/* override tabletQuickFix.css from globals */
@media only screen and (max-device-width: 1024px) and (min-device-width: 600px) and (orientation: portrait) {
    body div#page {
        font-size: 1em;
    }
}
@media only screen and (max-width : 600px){
	
	.default.containsImagelessArticle .article a,
    .default .span3.oddNumberArticles .article a,
    .default .span2.oddNumberArticles .article a,
    .default .span1.oddNumberArticles .article a,
    .default .fullBelow480 .article a {
        color:#CF4631;
    }
	
}
@media only screen and (max-device-width: 1024px) and (min-device-width: 600px) {
    #page #header {
        margin-top: 0;
    }
}
@media only screen and (min-width: 601px)
{
    #page .gridUnit > .gridRow {
        padding-top: 1.875em;
    }
}
@media only screen and (max-device-width: 1024px) {
    body #page,
    body #preFooter {
        width: 100%;
    }
}

@media only screen and (max-width: 599px) and (min-width: 481px){
    
     .tipOssMobile, .eAvisMobile{
        margin-left:1.2em;
     }
    .logoMobile, .sideMenuMobile,.weatherMobile,  .tipOssMobile, .eAvisMobile{
       width: auto;
       
    }
    .weatherMobile{
        float:right;
        margin-right: 0.1em;
    }
}

@media only screen and (max-width: 480px){
    
    .tipOssMobile, .eAvisMobile{
        display:none ;
    }
    .logoMobile, .sideMenuMobile,  .weatherMobile{
       width: auto;
    }
    .weatherMobile{
        float:right;
        margin-right: 0.1em;
    }
}
