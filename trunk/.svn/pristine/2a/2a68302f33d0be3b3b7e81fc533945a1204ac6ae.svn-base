/* mobile dialog box */
#mnoDialogBg {
    background: black;
    background: rgba(0, 0, 0, .8);
    filter: alpha(opacity=80);
    position: absolute;
    top: 0;
    left: 0;
    z-Index: 1000;
    width: 100%;
    height: 100%;
}
#mnoDialog {
    background: white;
    border: 5px solid #666;
    border: 5px solid rgba(0, 0, 0, .5);
    -moz-border-radius: 12px;
    -webkit-border-radius: 12px;
    border-radius: 12px;
    -moz-box-shadow: 0 0 20px #000;
    -webkit-box-shadow: 0 0 20px black;
    box-shadow: 0 0 20px black;
    position: fixed;
    z-index: 1001;
    padding: 10px;
    overflow-x: visible;
}
#mnoDialogClose{
    background:#333;
    border:2px solid #fff;
    -moz-border-radius:12px;
    -webkit-border-radius:12px;
    border-radius:12px;
    -moz-box-shadow:0 1px 2px #000;
    -webkit-box-shadow:0 1px 2px #000;
    box-shadow:0 1px 2px #000;
    color:#fff;
    cursor:pointer;
    font-size:14px;
    font-weight:bold;
    line-height:20px;
    text-shadow:0 1px 2px #000;
    text-align:center;
    position:absolute;
    right:-15px;
    top:-15px;
    width:20px;
    height:20px
}
/* Mobileswipe END. Move to own CSS */
