.rssFull .header {
    background: 0;
    height: auto;
    margin-bottom: 0;
    moz-box-shadow: none;
    -webkit-box-shadow: none;
    box-shadow: none;
}

.rssFull .header h2 {
    float:none;
    margin-top:0;
}

.rssFull .thumb img{
    float: left;
    margin-right: 10px;
    width:75px;
}

/* Aenergy */
.AenergyForeigner{
    border: 1px solid #ddd;
    padding-left: 5px;
}

.AenergyForeigner h3{
    font-size: 13px;
    font-family: sans-serif;
}

.AenergyForeigner p.teaserText,
.eksterneFeeds h3,
.eksterneFeeds p{
    font-size: 11px !important;
    font-family: sans-serif;
}

.AenergyForeigner li,
.eksterneFeeds li{
    border-bottom: 0;
}

.eksterneFeeds{
    background-color: #f4f6f8;
}