    color: #000;
}


/* Smartphones (portrait and landscape) ----------- */
@media only screen and (max-device-width : 960px) {
    .widget.livestudio.stream .informationItem .itemContent q {
        /* mobile css already adds quotes */
        quotes: none !important;
    }
    .widget.livestudio.stream .informationItem .itemContent q:before {
        content: ""; /* remove quotes */
    }
    .widget.livestudio.stream .informationItem .itemContent q:after {
        content: ""; /* remove quotes */
    }
}

/* mobile version */
.mobileStoryContent .widget.livestudio.stream .items .informationItem > img {
    display: block;
    float: none !important;
    width: auto;
    height: auto;
}
.mobileStoryContent .widget.livestudio.stream .items .informationItem {
    padding: 10px 15px 10px 5px !important;
    font-size: 14px;
}
.mobileStoryContent .widget.livestudio.stream .informationItem header {
    margin: -11px 0 4px -10px;
}