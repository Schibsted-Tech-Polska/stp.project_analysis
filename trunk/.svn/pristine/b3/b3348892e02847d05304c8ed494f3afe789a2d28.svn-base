package com.escenic.framework.community.struts.forms;

import com.ndc.usercontent.struts.actions.forms.ArticleForm;

/*
 * This is a custom ArticleForm extending ArticleForm provided by community engine
 * The purpose of this modified form is to handle editing related picture
 *
 * @see com.ndc.usercontent.struts.actions.forms.AbstractArticleForm;
 * @see com.ndc.usercontent.struts.actions.forms.ArticleForm
 *
 * @author <a href="mailto:ashis@escenic.com">Ashis Saha</a>
 * @version $Revision: #1 $
 *
 */
public class CustomArticleForm extends ArticleForm {

  private String mImage;

  public String getImage() {
		return mImage;
	}

	public void setImage(String pImage) {
		super.setImage(pImage);
    mImage = pImage;
  }


}
