.widget.classifiedBox.overview.mobile header {
    display: none;
}

.widget.classifiedBox.overview .resultListBox .resultItemResultList li {
    background: #F2F2F2;
    border-bottom: 3px solid #000066;
    height: 430px;
}

.widget.classifiedBox.overview .resultListBox .resultItemThumb {
    border-bottom: 1px solid #000066;
}

.widget.classifiedBox.overview .resultListBox .resultItemLogo {
    border: 1px solid #FFFFFF;
    display: block;
    margin: 0 auto;
    position: relative;
    top: -15px;
}

.widget.classifiedBox.overview .resultListBox .resultItemText {
    color: #666666;
    display: block;
    font-size: 10pt;
    height: 40px;
    padding: 0 10px;
    text-align: center;
}

.widget.classifiedBox.overview .resultListBox .resultItemAddress {
    color: #A3A3A3;
    font-style: oblique;
    display: block;
    font-size: 10pt;
    padding: 15px 10px 5px 10px;
    text-align: center;
}

.widget.classifiedBox.overview .resultListBox .resultItemPrice {
    color: #000066;
    font-weight: bold;
    font-size: 24pt;
    padding: 0 10px;
    text-align: center;
    display: block;
}

.widget.classifiedBox.overview .resultListBox .properties {
    background: none;
    border-color: #ddd;
    border-style: solid;
    border-width: 1px 0 1px 0;
    height: 34px;
    margin: 10px;
    padding: 2px;
    width: 290px;
    text-align: center;
}
.widget.classifiedBox.overview .resultListBox .properties span {
    color: #A3A3A3;
    font-size: 14pt;
    padding-right: 15px;
}

.widget.classifiedBox.overview .resultListBox .properties span strong {
    color: #666666;
    font-weight: bold;
    font-size:  16pt;
}

.widget.classifiedBox.overview .categories {
    background: #F2F2F2 !important;
    border-width: 0px 0px 3px 0px !important;
    border-style: solid !important;
    border-color: #000066;
}
.widget.classifiedBox.overview .categories li a, .widget.classifiedBox.overview .categories li span, .widget.classifiedBox.overview .categories li strong {
    border-left: 0 !important;
}

.widget.classifiedBox.overview .resultListBox .pagelinks li {
    background: #123466;
    background: -moz-linear-gradient(top, #3D568F 0%, #123466 100%);
    background: -webkit-linear-gradient(top, #3D568F 0%, #123466 100%);
    background: -o-linear-gradient(top, #3D568F 0%, #123466 100%);
    background: -ms-linear-gradient(top, #3D568F 0%, #123466 100%);
    background: linear-gradient(top, #3D568F 0%, #123466 100%);
    border-left: 1px solid rgba(35, 56, 122, 0.4);
    border-bottom: 1px solid rgba(35, 56, 122, 0.4);
    border-right: 1px solid rgba(64, 139, 196, 0.4);
    border-top: 1px solid rgba(64, 139, 196, 0.4);
}

.widget.classifiedBox.overview .resultListBox .pagelinks li.current,
.widget.classifiedBox.overview .resultListBox .pagelinks li:hover{
    background: #408BC4;
    background: -moz-linear-gradient(top, #408BC4 0%, #23387A 100%);
    background: -webkit-linear-gradient(top, #408BC4 0%, #23387A 100%);
    background: -o-linear-gradient(top, #408BC4 0%, #23387A 100%);
    background: -ms-linear-gradient(top, #408BC4 0%, #23387A 100%);
    background: linear-gradient(top, #408BC4 0%, #23387A 100%);
}

/* ----------------------- Global mobile css -------------------- */

.widget.classifiedBox.overview.mobile {
    float: left;
    width: 100%;
    padding: 0!important;
}

.widget.classifiedBox.overview.mobile .header {
    display: block;
}

.widget.classifiedBox.overview.mobile .resultItemResultList {
    float: left;
    width: 100%;
}

.widget.classifiedBox.overview.mobile .resultItemResultList li {
    float: left;
    width: 100%;
    background-color: #f2f2f2;
    border-bottom: 1.5px solid #142f5b;
    height: 105px;
    margin: 0 0 10px 0;
    overflow: visible;
    position: relative;
}

.widget.classifiedBox.overview.mobile .resultItemResultList li div:before {
    background: rgba(0, 0, 0, 0.1);
    background: -moz-linear-gradient(top, rgba(0, 0, 0, 0.1) 0%, rgba(0, 0, 0, 0) 100%);
    background: -webkit-linear-gradient(top, rgba(0, 0, 0, 0.1) 0%, rgba(0, 0, 0, 0) 100%);
    background: -o-linear-gradient(top, rgba(0, 0, 0, 0.1) 0%, rgba(0, 0, 0, 0) 100%);
    background: -ms-linear-gradient(top, rgba(0, 0, 0, 0.1) 0%, rgba(0, 0, 0, 0) 100%);
    background: linear-gradient(top, rgba(0, 0, 0, 0.1) 0%, rgba(0, 0, 0, 0) 100%);
    width: 100%;
    height: 8px;
    content: '';
    position: absolute;
    top: 0;
    right: 0;
}

.widget.classifiedBox.overview.mobile .resultItemResultList li .left, .widget.classifiedBox.overview .resultItemResultList li .right {
    float: left;
    width: 50%;
    height: 100%;
    position: relative;
    text-align: center;
}
@media (orientation: landscape) {
    .widget.classifiedBox.overview .resultItemResultList li .left {
        width: 30%;
    }
}

.widget.classifiedBox.overview.mobile .resultItemResultList li .right {
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    background-color: #f2f2f2;
    padding: 12px;
}
@media (orientation: landscape) {
    .widget.classifiedBox.overview.mobile .resultItemResultList li .right {
        float: right;
        width: 65%;
    }
}

.widget.classifiedBox.overview.mobile .resultItemResultList li img {
    margin: 0;
}

.widget.classifiedBox.overview.mobile .resultItemResultList li .resultItemThumb {
    height: 100%;
    margin: 0 auto;
    width: auto;
    border-right: 1px solid gray;
}

.widget.classifiedBox.overview.mobile .resultItemResultList li .resultItemLogo {
    border: 1px solid white;
    height: 14px;
    margin: -8px auto 0 auto;
    position: static;
    width: auto;
}

.widget.classifiedBox.overview.mobile .resultItemResultList li span {
    float: left;
    width: 100%;
    overflow: hidden;
}

.widget.classifiedBox.overview.mobile .resultItemResultList li .resultItemText {
    color: #595959;
    font-size: 9px;
    line-height: 10px;
    max-height: 30px;
    padding: 0;
}

.widget.classifiedBox.overview.mobile .resultItemResultList li .resultItemAddress {
    color: #999999;
    font-size: 9px;
    line-height: 12.5px;
    margin: 10px 0;
    max-height: 12.5px;
    padding: 0;
}

.widget.classifiedBox.overview.mobile .resultItemResultList li .resultItemPrice {
    color: #142f5b;
    font-size: 18px;
    line-height: 18px;
    max-height: 18px;
    padding: 0;
}

.widget.classifiedBox.overview.mobile .pagelinks {
    float: left;
    width: 100%;
    background: #2771aa;
    background: -moz-linear-gradient(top, #2771aa 0%, #122358 100%);
    background: -webkit-linear-gradient(top, #2771aa 0%, #122358 100%);
    background: -o-linear-gradient(top, #2771aa 0%, #122358 100%);
    background: -ms-linear-gradient(top, #2771aa 0%, #122358 100%);
    background: linear-gradient(top, #2771aa 0%, #122358 100%);
    font-family: 'ClanWeb',Arial,sans-serif;
    padding: 0;
    margin: 0;
    text-align: center;
}

.widget.classifiedBox.overview.mobile .pagelinks li {
    display: inline-block;
    background: none!important;
    border: 0!important;
    margin: 0;
    text-shadow: none;
    box-shadow: none;
    border-radius: 0;
}

.widget.classifiedBox.overview.mobile .pagelinks li.current a {
    background: #408bc4;
    background: -moz-linear-gradient(top, #408bc4 0%, #23387a 100%);
    background: -webkit-linear-gradient(top, #408bc4 0%, #23387a 100%);
    background: -o-linear-gradient(top, #408bc4 0%, #23387a 100%);
    background: -ms-linear-gradient(top, #408bc4 0%, #23387a 100%);
    background: linear-gradient(top, #408bc4 0%, #23387a 100%);
}

.widget.classifiedBox.overview.mobile .pagelinks li a, .widget.classifiedBox.overview.mobile .pagelinks li.skip {
    border-right: 1px solid rgba(64, 139, 196, 0.4);
    border-left: 1px solid rgba(35, 56, 122, 0.4);
    color: white!important;
    display: block;
    font-size: 12px;
    font-weight: normal!important;
    line-height: 35px;
    padding: 0 5px;
    width: 14px;
}

.widget.classifiedBox.overview.mobile .pagelinks li.skip {
    display: inline-block;
}

.widget.classifiedBox.overview.mobile .categories select {
    float: right;
}

.gridUnit .widget.classifiedBox.overview.mobile .loader {
    background: none;
    width: 16px;
    height: 11px;
}