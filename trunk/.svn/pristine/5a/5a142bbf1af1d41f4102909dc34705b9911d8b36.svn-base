<ul class="resultItemResultList">
{{each items}}
<li>
    {{if (price !== undefined && price !== '0' && price !== null)}}
        <div class="resultItemPrice">{{html price}},-</div>
    {{/if}}
    {{if (defaultImageUrl)}}
    <img src="{{html defaultImageUrl}}" alt="{{html head}}">
    {{/if}}
    <h3>{{html heading}}</h3>
    <a href="{{if typeof(url)=='string'}}{{html url}}{{else}}{{html landingUrl}}{{html objectId}}{{/if}}">les mer om cruiset her</a>
</li>
{{/each}}
</ul>