
.widget.classifiedBox.overview {
    float: left;
    width: 100%;
    padding: 20px 0 0 0 !important;
}

.widget.classifiedBox.overview .header {
    display: none !important;
}

.widget.classifiedBox.overview .resultItemResultList {
    float: left;
    width: 100%;
}

.widget.classifiedBox.overview .resultItemResultList li {
    float: left;
    width: 100%;
    background-color: #f2f2f2;
    border-bottom: 1.5px solid #142f5b;
    height: 105px;
    margin-bottom: 10px;
    position: relative;
}

.widget.classifiedBox.overview .resultItemResultList li div:before {
    background: rgba(0, 0, 0, 0.1);
    background: -moz-linear-gradient(top, rgba(0, 0, 0, 0.1) 0%, rgba(0, 0, 0, 0) 100%);
    background: -webkit-linear-gradient(top, rgba(0, 0, 0, 0.1) 0%, rgba(0, 0, 0, 0) 100%);
    background: -o-linear-gradient(top, rgba(0, 0, 0, 0.1) 0%, rgba(0, 0, 0, 0) 100%);
    background: -ms-linear-gradient(top, rgba(0, 0, 0, 0.1) 0%, rgba(0, 0, 0, 0) 100%);
    background: linear-gradient(top, rgba(0, 0, 0, 0.1) 0%, rgba(0, 0, 0, 0) 100%);
    width: 100%;
    height: 7.5px;
    content: '';
    position: absolute;
    top: 0;
    right: 0;
}

.widget.classifiedBox.overview .resultItemResultList li .left, .widget.classifiedBox.overview .resultItemResultList li .right {
    float: left;
    width: 50%;
    height: 100%;
    position: relative;
    text-align: center;
}
@media (orientation: landscape) {

    .widget.classifiedBox.overview .resultItemResultList li .left {
        width: 50%;
    }
}

.widget.classifiedBox.overview .resultItemResultList li .right {
    background-color: #f2f2f2;
    box-sizing: border-box;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    padding: 12px;
}
@media (orientation: landscape) {

    .widget.classifiedBox.overview .resultItemResultList li .right {
        float: right;
        width: 45%;
    }
}

.widget.classifiedBox.overview .resultItemResultList li img {
    margin: 0;
}

.widget.classifiedBox.overview .resultItemResultList li .resultItemThumb {
    height: 100%;
    margin: 0 auto;
    width: auto;
    border-right: 1px solid gray;
}

.widget.classifiedBox.overview .resultItemResultList li .resultItemLogo {
    border: 1px solid white;
    height: 14px;
    margin: -7.5px auto 0 auto;
    width: auto;
}

.widget.classifiedBox.overview .resultItemResultList li span {
    float: left;
    width: 100%;
    overflow: hidden;
}

.widget.classifiedBox.overview .resultItemResultList li .resultItemText {
    color: #595959;
    font-size: 9px;
    line-height: 10px;
    max-height: 30px;
}

.widget.classifiedBox.overview .resultItemResultList li .resultItemAddress {
    color: #999999;
    font-size: 9px;
    line-height: 12.5px;
    margin: 10px 0;
    max-height: 12.5px;
}

.widget.classifiedBox.overview .resultItemResultList li .resultItemPrice {
    color: #000;
    font-size: 17.5px;
    line-height: 17.5px;
    max-height: 17.5px;
}

.widget.classifiedBox.overview .pagelinks {
    float: left;
    width: 100%;
    background: #000 !important;
    font-family: 'ClanWeb',Arial,sans-serif;
    padding: 0;
    margin: 0;
    text-align: center;
}

.widget.classifiedBox.overview .pagelinks li {
    display: inline-block;
}

.widget.classifiedBox.overview .pagelinks li.current a {
    background: #666 !important;
}

.widget.classifiedBox.overview .pagelinks li a, .widget.classifiedBox.overview .pagelinks li.skip {
    border-right: 1px solid #FFF;
    border-left: 1px solid rgba(35, 35, 35, 0.4);
    color: white;
    display: block;
    font-size: 12px;
    line-height: 35px;
    padding: 0 5px;
    width: 14px;
}

.widget.classifiedBox.overview .pagelinks li.skip {
    display: inline-block;
}

.widget.classifiedBox.overview .categories select {
    float: right;
}


body > section:last-child {
    display: none;
}


.gridUnit .widget.classifiedBox.overview .loader {
    background: none;
    width: 16px;
    height: 11px;
}
