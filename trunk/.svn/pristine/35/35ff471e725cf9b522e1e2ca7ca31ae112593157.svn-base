	top:0;
	left: 60px;
}

.right-panel{
    float: right;
    clear: right;
}
.social {
    float: right;
}
#header a.twitter, #header a.instagram, #header a.facebook {
    background-size: 22px 22px;
    height: 22px;
    width: 22px;
    left: 0;
    margin-left: 10px;
    margin-top: 5px;
}
#header .topMainMenu {
    float: right;
    margin-top: 0;
}

