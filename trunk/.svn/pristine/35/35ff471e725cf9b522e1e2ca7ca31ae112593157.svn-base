}

#header {
    background-color: #000000;
    margin: 0 0 10px;
    padding: 0;
    width: 100%;
}
.ie9 #header  {
    filter: none;
}
#header .menuWrapper {
    clear: both;
    background-color: #000000;
    border-top: 1px solid #000000;
    height: auto;
}

#viewport #header {
    border: 1px solid #bbb;
    border-bottom: 0 none;
    padding-top: 10px;
}
.specialSpan130 {
    width: 130px;
}

.specialSpan140 {
    width: 140px;
}

.specialSpan840 {
    margin-left: 10px;
    width: 840px;
}

#header .subMenu {
    background-color: #fff;
    text-align: right;
    padding-top: 9px;
    margin-top: 0;
}

#header .widget.logo {
    height: 75px;
}

#header .widget.logo img {
    margin-top: 14px;
    width: 234px;
    height: 60px;
}

/* infoMenu bar */
#header .infomenuWrapper {
    clear: both;
    background-color: #eeeeee;
    border-top: 1px solid #eeeeee;
    height: 24px;
    margin-top: 0;
}
#header .infomenuWrapper .standardMenu:after {
    clear: none;
}
.top-ornament {
    position: relative;
}
.top-ornament:before {
    content: "";
    display: block;
    position: absolute;
    left: 0;
    right: 0;
    top: -10px;
    height: 1px;
    border-top: 1px dashed #231f20;
}
.farsund-logo {
    width: 170px;
    color:#aba9a9;
    font-size: 12px;
}
.farsund-logo p {
    margin: 4px 0 0;
    line-height: 24px;
    text-align: left;
    font-weight: bold;
}
.farsund-logo strong {
    background: url("/skins/publications/farsund/gfx/logos_sprite.png") no-repeat 0 -77px;
    text-indent: 100%;
    white-space: nowrap;
    overflow: hidden;
    width: 77px;
    height: 18px;
    display: inline-block;
    vertical-align: middle;
    margin-left: 5px;
}

/* logo bar/ main header area  */
.mainHeaderArea .span2 {
    position: relative;
    height: 77px;
}
.mainHeaderArea .related-cities {
    text-align: right;
}
.mainHeaderArea .dateline {
    text-align: left;
    color: #838383;
    font-size: 14px;
    text-transform: lowercase;
    margin-top: 8px;
}

/* menu wrapper bar */
.contact-us {
    text-align: left;
    vertical-align: middle;;
}
.contact-us span {
    display: inline-block;
    border-radius: 9px;
    line-height: 18px;
    padding: 0 12px;
    background-color: #fd0501;
    color: #fff;
    font-size: 14px;
    font-weight: bold;
    text-transform: uppercase;
}

/* shorthandBar */
.shorthandBar {
    background-color: #ececec;
    line-height: 24px;
}
.shorthandBar .span4 {
    text-align: right;
    height: 24px;
}
.shorthandBar .searchField {
    display: inline-block;
    vertical-align: top;
}
.shorthandBar .searchField .pillBox {
    text-align: left;
    border-radius: 0;
    border: 0 none;
    background-color: #fff;
    color: #000;
    font-size: 12px;
    width: 224px;
    height: 20px;
    margin-top:2px;
    overflow: visible;
}
.shorthandBar .searchField .pillBox input {
    border-radius: 0;
    background-color: #fff;
    color: #000;
    box-shadow: none;
    height: 20px;
    padding: 0 8px;
    width: 224px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.shorthandBar .searchField .pillBox input:focus {
    box-shadow: 0 0 2px 0px rgba(0, 0, 0, 0.4);
}
.shorthandBar .searchField .pillBox button {
    visibility: hidden;
}

/* breaking news */
.breakingnews {
    font-size: 14px;
    font-weight: bold;
    text-transform: uppercase;
    color: #f50400;
}
.breakingnews a {
    color:#000;
    font-weight: normal;
    text-transform: none;
}

