#page {
    border: 0 none;
    -moz-box-shadow: none;
        -webkit-box-shadow: none;
        box-shadow: none;
    margin-right:-3px;
    padding-right: 0;
}

#page:before,
#page:after {
    display:none;
}

#header .mainHeader,
#header .sectionHeader {
    background-color: #000a18;
        border-bottom: 1px solid #000;
    height:90px;
    position:relative;
}

#header .mainHeader .grid12,
#header .sectionHeader .grid12 {
    background-image: url('/skins/global/gfx/highlight.jsp?x1=50&y1=0&r=50&colorStop=2e87a7|0|0.8,2e87a7|100|0&w=200&h=275&rotDeg=340');
    background-repeat:no-repeat;
    background-position:140px 0;
}

.latestTicker {
    border-bottom: 1px solid #333;
}

#header .gridRow.mainHeader,
#header .gridRow.sectionHeader,
#header .gridRow.latestTicker {
    margin-top: 0;
}

#smallHeader:after {
    background-image: -moz-radial-gradient(50% 0 , farthest-corner, rgba(52, 170, 194, 0.6), transparent);
    content: " ";
    height: 30px;
    position: absolute;
        left: 0;
        top: 0;
    visibility: visible;
    width: 1180px;
}

#smallHeader .logo {
    margin-right:3px;
    position: relative;
	z-index: 2;
}

