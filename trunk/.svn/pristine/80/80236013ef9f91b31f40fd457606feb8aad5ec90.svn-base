input[type=submit],
input[type=button],
button,
.button {
    background-color: #333;
    padding-left:7px;
    padding-right:7px;
}

input[type=text],
input[type=color],
input[type=date],
input[type=datetime],
input[type=datetime-local],
input[type=email],
input[type=file],
input[type=image],
input[type=month],
input[type=number],
input[type=password],
input[type=range],
input[type=reset],
input[type=search],
input[type=tel],
input[type=text],
input[type=time],
input[type=url],
input[type=week],
input[type=button],
input[type=submit],
input.text,
button,
select,
textarea,
.button,
.pillBox {
    -moz-border-radius:15px;
        -webkit-border-radius:15px;
        border-radius:15px;
}

.pillBox input,
.pillBox button,
.pillBox .button {
    -moz-border-radius:0;
        -webkit-border-radius:0;
        border-radius:0;
}

.pillBox .button {
    padding:0 4px;
}

.pillBox *:first-child,
.pillBox .pillBox:first-child *:first-child,
.pillBox .first {
    -moz-border-radius: 14px 0 0 14px;
        -webkit-border-radius: 14px 0 0 14px;
        border-radius: 14px 0 0 14px;
    padding-left:7px;
}

.ie .pillBox div,
.ie .pillBox .button,
.ie .pillBox input {
    border-top: 0 none;
    border-bottom: 0 none;
    padding-left:4px;
    padding-right:4px;
}

.pillBox *:last-child,
.pillBox .last {
    -moz-border-radius: 0 14px 14px 0;
        -webkit-border-radius: 0 14px 14px 0;
        border-radius: 0 14px 14px 0;
    padding-right:7px;
}

.pillBox .pillBox,
.pillBox .pillBox:first-child,
.pillBox .pillBox:last-child {
    padding: 0;
}

.ie .pillBox .pillBox {
    padding: 0;
}

.pillBox .pillBox:last-child *:last-child{
    -moz-border-radius: 0 14px 14px 0;
        -webkit-border-radius: 0 14px 14px 0;
        border-radius: 0 14px 14px 0;
    padding-right:7px;
}

.pillBox .pillBox:first-child *:first-child{
    -moz-border-radius: 14px 0 0 14px;
        -webkit-border-radius:14px 0 0 14px;
        border-radius: 14px 0 0 14px;
    padding-left:7px;
}

