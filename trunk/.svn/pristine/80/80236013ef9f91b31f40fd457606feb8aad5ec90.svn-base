/* 2 - Typography */

body {
    color: #121212;
    font-family: Arial, sans-serif;

}

h1,
h2 {
    font-family: 'publicoHeadlineWeb', Cambria, Times, 'Times New Roman', serif;
    font-weight:bold;
}

.ie h1,
.ie h2 {
    font-family: 'publicoHeadlineWebBold', Cambria, Times, 'Times New Roman', serif;
}

.span2 h1,
.span2 h2,
.span1 h1,
.span1 h2 {
    font-family: 'ClanWeb', Arial, sans-serif;
        letter-spacing:-.4px;
    margin-top:5px;
}

.ie .span2 h1,
.ie .span2 h2,
