<section class="smb-full">
  <a href="${adUrl}">

    <img src="${siteLogo}" class="nyttig" />

        <img class="ad" src='http://${lisaLevel}.aftenposten.no/utils/img.php?src=${imgUrl}${adId}_${adImage}&amp;width=${maxWidth}' alt='${title}'/>
    <h2>${title}</h2>
    {{if oldPrice && newPrice}}
    <p>
        N&aring;: <span>${newPrice} kr</span>
        {{if discount}}
            <span class="discount">${discount}</span>
        {{/if}}
    </p>
    <p class="before">
        ${oldPrice} kr
    </p>
    {{else newPrice}}
    <p>
        N&aring;: <span>${newPrice} kr</span>
        {{if discount}}
            <span class="discount">${discount}</span>
        {{/if}}
    </p>
    {{else}}
    <p>
        Se tilbudet
        {{if discount}}
            <span class="discount">${discount}</span>
        {{/if}}
    </p>
    {{/if}}
    {{if compLogo}}
       <img class="logo" src='http://${lisaLevel}.aftenposten.no/utils/img.php?src=${logoUrl}${adId}_${compLogo}&amp;width=100' alt='${title}'/>
    {{/if}}
  </a>
<section>