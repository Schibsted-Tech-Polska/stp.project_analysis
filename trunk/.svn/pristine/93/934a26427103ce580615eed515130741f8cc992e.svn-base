/* 1 - Layout */

#smallHeader .logo {
    margin-right:3px;
}

#specialHeader-logo {
    background: #014687;
    line-height:25px;
    margin-right:5px;
    text-align:center;
    width:25px;
    height:25px;
}

.specialHeader img,
#specialHeader-logo img {
    vertical-align:middle;
}

.specialHeader {
    height:99px;
}

.specialHeader .logo {
    height:99px;
    line-height:99px;
}

.bergenpuls .specialHeader {
    background:url(/skins/publications/bt/gfx/themes/bergenpuls.jpg) no-repeat bottom;
}

section header,
article header {
    border-bottom: 3px solid #014687;
    margin-bottom: 5px;
}

.bluePane {
    background:#e5ecf3;
    border:1px solid #b2c7db;
    -moz-border-radius:3px;
    -webkit-border-radius:3px;
    border-radius:3px;
    padding:10px;
}

.bluePane header {
    border-bottom:0 none;
}

.relatedPicture {
    background: #f0f0f0;
}

.relatedArticle h3 {
    margin: 15px 0 5px;
}

.relatedArticle header {
    margin-bottom: 0;
}

.relatedArticle .content {
    background: #f0f0f0;
}

.relatedContents .hmedia {
    margin-top: 5px;
}

.subTitle {
    padding-left: 20px;
    margin: 10px 0 15px;
}

.subTitle:before {
    background: #999;
    content: " ";
    float: left;
    margin-left: -20px;
    margin-top: 5px;
    width: 15px;
    height: 15px;
}

.ie .subTitle {
    border-left: 15px solid #999;
    padding-left: 5px;
}

section header h2,
article header h2,
section header h3,
article header h3 {
    margin-bottom: 0;
}

input[type=submit],
input[type=button],
button,
.button {
    background-color: #014687;
}

.artTheme.negativ.noThemeHeadline .tipUs a,
.artTheme.negativ.noThemeHeadline .tipUs a:hover,
.artTheme.negativ.noThemeHeadline .tipUs a:visited,
.artTheme.negativ.noThemeHeadline .tipUs a:active,
.artTheme.negativ.hasThemeHeadline .tipUs a,
.artTheme.negativ.hasThemeHeadline .tipUs a:hover,
.artTheme.negativ.hasThemeHeadline .tipUs a:visited,
.artTheme.negativ.hasThemeHeadline .tipUs a:active,
.rowTheme.negativ.noThemeHeadline .tipUs a,
.rowTheme.negativ.noThemeHeadline .tipUs a:hover,
.rowTheme.negativ.noThemeHeadline .tipUs a:visited,
.rowTheme.negativ.noThemeHeadline .tipUs a:active,
.rowTheme.negativ.hasThemeHeadline .tipUs a,
.rowTheme.negativ.hasThemeHeadline .tipUs a:hover,
.rowTheme.negativ.hasThemeHeadline .tipUs a:visited,
.rowTheme.negativ.hasThemeHeadline .tipUs a:active{
    color: #014687;
}
.artTheme.negativ.noThemeHeadline .tipUs .prefix a,
.artTheme.negativ.noThemeHeadline .tipUs .prefix a:visited,
.artTheme.negativ.hasThemeHeadline .tipUs .prefix a,
.artTheme.negativ.hasThemeHeadline .tipUs .prefix a:visited,
.rowTheme.negativ.noThemeHeadline .tipUs .prefix a,
.rowTheme.negativ.noThemeHeadline .tipUs .prefix a:visited,
.rowTheme.negativ.hasThemeHeadline .tipUs .prefix a,
.rowTheme.negativ.hasThemeHeadline .tipUs .prefix a:visited {
    color: #fff;
}

.negativ.bgColorOuter{
    background-color: #222;
}

#colophon {
    background: #f9f9f9;
    filter:  progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#00000000', endColorstr='#10000000');
    -ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#00000000', endColorstr='#10000000')";
    background-image: -moz-linear-gradient(top, rgba(0,0,0,0.0), rgba(0,0,0,.05));
    background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0, rgba(0,0,0,0)),color-stop(1, rgba(0,0,0,.05)));
    background-image: -webkit-linear-gradient(top, rgba(0,0,0,0), rgba(0,0,0,.05));
    background-image: linear-gradient(top, rgba(0,0,0,0), rgba(0,0,0,.05));
    border:1px solid #ccc;
    font-size:12px;
    overflow:hidden;
    position:relative;
}

#colophon h2 {
    font-size:16px;
}
#colophon dl {
    position:relative;
}
#colophon dt {
    position:absolute;
    left:0;
    text-align:right;
    width:170px;
}

#colophon dd {
    margin-left:180px;
}

/* 2 - Typography */

body {
    color: #121212;
    font-family: Arial, sans-serif;

}

h1,
h2 {
    font-family: Cambria, Times, 'Times New Roman', serif;
}

a {
    color: #014687;
}

a:hover {
    -moz-transition:all 250ms ease-in;
    -webkit-transition:all 250ms ease-in;
    -ms-transition:all 250ms ease-in;
    -khtml-transition:all 250ms ease-in;
    -o-transition:all 250ms ease-in;
    transition:all 250ms ease-in;
    text-decoration:underline;
}

h1 a:hover,
h2 a:hover,
h3 a:hover,
h4 a:hover {
    color:#333;
    text-decoration:none;
}

form label {
    font-size: 12px;
}

section .header h1,
article .header h1,
section .header h2,
article .header h2,
section .header h3,
article .header h3 {
    color: #000;
    font-family: Arial, sans-serif;
    font-weight: bold;
    font-size: 12px;
    text-transform: uppercase;
}

.span1 section .header h2,
.span1 section .header h3,
.span2 section .header h2,
.span2 section .header h3,
.span1 article .header h2,
.span1 article .header h3,
.span2 article .header h2,
.span2 article .header h3 {
    font-size:10px;
}

/*TODO : FLyttes til egen CSS*/
.lap2 header {
    text-align: right;
}

section footer,
article footer {
    font-weight: bold;
    text-transform: uppercase;
}

section footer,
article footer {
    font-size: 10px;
    line-height:1;
}

section footer a,
article footer a {
    border-right:1px solid #999;
    margin-right:5px;
    padding-right:5px;
}

section footer a:last-child,
article footer a:last-child {
    border-right:0 none;
}

section footer a.lastChild,
article footer a.lastChild{
    border-right:0 none;
}

.relatedArticle h3 {
    font-family: Cambria, Times, 'Times New Roman', serif;
    font-size: 18px;
}

h2.subTitle {
    font-family: arial, sans-serif;
    font-size: 24px;
}

/* 3 - Events */
body.ie6 {
    behavior: url(/skins/global/csshover3.htc);
}

#viewport {
    min-height: 1000px;
}

/* ie uses behaviors for round corner */
.roundCorner {
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    -moz-background-clip: padding;
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
}

.roundCornerBottom {
    -moz-border-radius: 0 0 3px 3px;
    -webkit-border-bottom-right-radius: 3px;
    -webkit-border-bottom-left-radius: 3px;
    border-bottom-right-radius: 3px;
}

/* topInfo is first gridRow with informations links */
.topInfoGridRow {
    border: dotted 1px;
    margin-bottom: 10px !important;
    width: 978px;
}

.topInfoGridRow  .unit-topInfo {
    font-size:12px;
    padding: 5px 10px;
}

.topInfoGridRow .customerService-topInfo {
    background-color: #ff6600;
}

.topInfoGridRow a {
    color: #000;
}

.topInfoGridRow a:visited {
    color: #000;
}

.topInfoGridRow a:hover {
    text-decoration: underline;
}

.pillBox {
    width: auto;
}
#smallHeader .pillBox {
    margin-top: 2px;
}
#header .pillBox input,
#smallHeader .pillBox input,
#header .pillBox button,
#smallHeader .pillBox button {
    font-family : "Calibre", Arial, sans-serif;
    font-size: 12px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    height: 17px;
}
.ie8 #header .pillBox input,
.ie8 #smallHeader .pillBox input,
.ie8 #header .pillBox button,
.ie8 #smallHeader .pillBox button {
    font-family:"CalibreIE", Arial, sans-serif;
}
#header .pillBox input,
#smallHeader .pillBox input {
    width:185px;
    padding: 0 3px;
    background-color: #fff;
    box-shadow: none;
}
#header .pillBox input:focus,
#smallHeader .pillBox input:focus {
    background-color: rgb(239,236,227);
}
#header .pillBox button,
#smallHeader .pillBox button {
    width: 28px;
    padding: 0;
    text-align: center;
    background-color: #1e2c50;
    border-radius: 0 2px 2px 0;
    font-weight: 500;
}

/* browser notification */
.ie6 .browserNotification h2,
.ie7 .browserNotification h2 {
    background:url(/skins/global/gfx/bt_logo_small.png) no-repeat 0 2px;
    padding-left:35px;
}
/* Adding uppercase to credits */
.credits {
    font-size:10px;
    font-weight: bold;
    margin-top:6px;
    text-transform: uppercase;
}

/* Tip us */
.ie7 .tipUs {
    display: inline;
    zoom:1;
}
.tipUs {
    display: inline-block;
    vertical-align:middle;
}

/* right ad banner */
#adRight {
    margin-left: 0;
}

.Calibre {
    font-family:"Calibre", arial, sans-serif;
}
.ie .Calibre {
    font-family:"CalibreIE";
}

/* lock icon on articles */
.media.block {
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
    box-sizing:border-box;
    position: relative;
}
.media.block .media {
    border: 0 none;
    margin: 0;
}

/* article teaser icons - lock, favourite */
.stories .teaserStatus {
    z-index: 2;
}

.stories .teaserStatus .icon {
    position: absolute;
}

.stories .teaserStatus .icon.lock.closed {
    top:5px;
    right:5px;
}

/* bread crumbs */
.bread-crumbs {
    float:left;
    font-size: 15px;
    font-weight: normal;
    color: #7a7a71;
    line-height: 1em;
}
.bread-crumbs a {
    color: #7a7a71;
    position: relative;
    padding-right: 11px;
    margin-right: 5px;
}
.bread-crumbs a:after {
    /* little arrow */
    content: '';
    display: block;
    position: absolute;
    top:35%;
    right:0;
    width: 0;
    height: 0;
    border-top: 4px solid transparent;
    border-left: 4px solid #7c7c71 ;
    border-bottom: 4px solid transparent;
}

/* BT dialog */
#bt #mnoDialog {
    border-radius: 0;
    border-width: 2px;
}
#mnoDialogContent h2 {
    margin-bottom: 0.5em;
}
#mnoDialogContent button {
    padding-left: 20px;
    padding-right: 20px;
    text-transform: uppercase;
}

/* store article */
.storearticle {
    text-align: center;
}

.storearticle a {
    width: 55px;
    height: 22px;
    background-image: url(/skins/publications/bt/gfx/icons/save.png);
    color: white;
    text-transform: uppercase;
    font-size: 0.81em;
    padding-top: 33px;
    margin-bottom: 10px;
    background-size: 100%;
    display: block;
    text-decoration: none;
    cursor: pointer;
}

/* Stored articles list */
.storedArticles.storedArticlesTrigger {
    float: right;
}

.storedArticles.storedArticlesTrigger .text {
    color: white;
    font-weight: 500;
    line-height: 21px;
    letter-spacing: .02em;
}

.storedArticles.storedArticlesTrigger .icon {
    opacity: 0;
    display: inline;
}

.storedArticles.storedArticlesTrigger .count {
    background-image: url(/skins/publications/bt/gfx/icons/star.png);
    display: inline-block;
    opacity: 1;
    color: white;
    height: 16px;
    width: 30px;
    background-position: 10px 2px;
    background-repeat: no-repeat;
}

.storedArticles.listArticles h2 {
    font-size: 20px;
}

.storedArticles.listArticles article {
    width:255px;
    margin-bottom: 25px;
}

.storedArticles.listArticles article h2 {
    font-family: "Calibre", Arial, sans-serif;
    font-weight: 500;
}

.storedArticles.listArticles article img {
    width: 65px;
}

.storedArticles.listArticles .info {
    margin: 5px 0px;
}

.storedArticles.listArticles .published, .storedArticles.listArticles .stored {
    font-size: 14px;
    line-height: 1em;
}

.storedArticles.listArticles .published, .storedArticles.listArticles .dates {
    width: 150px;
}

.storedArticles.listArticles .published, .storedArticles.listArticles .dates .f-bold{
    font-weight: normal;
}

/* icons - lock */

.icon.lock {
    display:none;
}
/* only "+" icon */
.icon.lock.closed {
    display: inline-block;
    background: url("/skins/publications/bt/gfx/icons/plus.png?v=2") no-repeat top right;
    display: inline-block;
    overflow: hidden;
    text-indent: -9999px;
    height: 16px;
    width: 17px;
}

/* "+logg pa" */
.hasNotAccess .media .icon.lock.closed {
    background-position: top left;
    width: 61px;
}
