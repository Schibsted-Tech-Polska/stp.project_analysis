/* 1 - Layout */
html,body {
    width:100%;
}

.nojs {
    -webkit-animation: bugfix infinite 1s;
}

@-webkit-keyframes bugfix {
    from { padding: 0; }
    to { padding: 0; }
}

h1,
h2,
h3,
h4 { /* h5, h6 are excluded */
    line-height:1;
    margin-bottom:.22em;
    text-rendering: optimizeLegibility;
    word-wrap:break-word;
}

p {
    margin-bottom:1em;
}

#surround {
    background:#fff;
        box-shadow: 0 0 20px rgba(0,0,0,.8);
}

#header,
#page,
#preFooter {
    background-color: #fff;
    padding-left:10px;
        padding-right:10px;
    margin-left:-10px;
    width:983px;
}

#page {
    background:#fff;
}

.sectionPage .collectionRow,
.sectionPage .gridRow {
    margin-bottom: 15px;
}

/* TODO FIkse denne så nesting på front ikker er feil */
.sectionPage .collectionRow .gridRow:last-child,
.sectionPage .gridRow .gridRow:last-child {
    margin-bottom: 0;
}

.pull1.relatedContents,
.pull1.relatedArticle,    
.pull1.hmedia {
    margin-bottom:10px;
}

/**
  Article list
  Basic styles for list
  @group lists
  @style
  .articleList Defines the list as an article list
  @example
  <ul class="articleList">
    <li><a href="#">Lorem ipsum dolor sit amet</a></li>
    <li><a href="#">Lorem ipsum dolor sit amet</a></li>
    <li><a href="#">Lorem ipsum dolor sit amet</a></li>
    <li><a href="#">Lorem ipsum dolor sit amet</a></li>
  </ul>
*/
.articleList {
    clear:both;
}

.articleList li {
    border-bottom:1px solid #ccc;
    margin-bottom:5px;
    overflow:hidden;
    padding-bottom:5px;
    width:100%;
}

/**
  Result list
  Basic styles for result list
  @group lists
  @style
  .resultList Defines the list as a result list
  .resultList-inner Helper element for positioning
  .resultList-number Add to element containing the number in the list
  .resultList.withImg Left align images in list. Use only if the list contain images.
  @example
  <ul class="resultList">
    <li>
        <div class="resultList-inner">
            <span class="resultList-number">1.</span>
            <a href="#">Lorem ipsum dolor sit amet</a>
        </div>
    </li>
    <li>
        <div class="resultList-inner">
            <span class="resultList-number">2.</span>
            <a href="#">Lorem ipsum dolor sit amet</a>
        </div>
    </li>
  </ul>
*/
.resultList {
    margin-top:0;
    padding-left:30px;
    padding-top:5px;
}

.resultList li {
    border-bottom:1px dotted #ccc;
    color:#666;
    font-size:16px;
        font-weight:bold;
    margin-bottom: 10px;
}

.resultList-inner {
    color:#121212;
    font-size:14px;
        font-weight:normal;
    position:relative;
}

.resultList-inner p {
    font-size:12px;
}

.resultList-number {
    color:#666;
    font-size:16px;
        font-weight:bold;
    position:absolute;
        top:-2px;
        left:-30px;
}

.resultList.withImg .resultList-inner {
    padding-left:80px;
}

.resultList.withImg img {
    position:absolute;
    top:5px;
    left:0;
}

.relatedArticle .left {
    margin-right:20px;
}

/**
  Pager
  Pager for lists
  @group lists
  @style
  .pager Creates a pager
  .button.active Set the active page
  @require /skins/global/grid.css
  @example
  <ol class="pager">
    <li>
        <a href="#" class="button">1</a>
    </li>
    <li>
        <a href="#" class="button">2</a>
    </li>
    <li>
        <span class="button active">3</span>
    </li>
  </ol>
*/
.pager {
    list-style:none;
    overflow:hidden;
    padding:5px;
    width:100%;
}
.pager li {
    display:-moz-inline-stack;
    display:inline-block;
    margin-bottom: 20px;
    margin-right:5px;
}
.ie .pager li {
    display:inline;
    position:relative;
}

.pager .button.active {
    background-color:#ccc;
    border:1px solid #666;
    color:#333;
}

