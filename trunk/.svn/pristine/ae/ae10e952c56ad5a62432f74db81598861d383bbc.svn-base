.jobAdListings2.categoryJobadsCombo header h2,
.jobAdListings2.mgmt header h2,
.jobAdListings2.profiled header h2,
.jobAdListings2.recruiting header h2 {
    background-color: transparent;
        background-image: none;
    color: #808080;
        font-family: Klavika, Helvetica, Arial, sans-serif;
        font-size: 22px;
        font-weight: normal;
        letter-spacing: normal;
        text-transform: uppercase;
        word-spacing: -5px;
    line-height: 30px;
    padding: 0!important;
}
.jobAdListings2.profiledCompanies header h2:before,
.jobAdListings2.categoryJobadsCombo header h2:before,
.jobAdListings2.mgmt header h2:before,
.jobAdListings2.profiled header h2:before,
.jobAdListings2.recruiting header h2:before {
