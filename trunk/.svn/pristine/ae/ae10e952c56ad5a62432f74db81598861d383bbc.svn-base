}

.jobAdListings2.profiledCompanies li > a,
.jobAdListings2.categoryJobadsCombo li > a,
.jobAdListings2.mgmt li > a,
.jobAdListings2.profiled li > a,
.jobAdListings2.recruiting li > a,
.jobAdListings2.recruiting li > div {
    background-color: #fafafa;
    float: left;
    line-height: 152px;
    text-align: center;
    text-decoration: none;
    width: 100%;
}
.jobAdListings2.categoryJobadsCombo li > a,
.jobAdListings2.mgmt li > a,
.jobAdListings2.profiled li > a {
    border-bottom: 4px solid #000;
    line-height: 136px;
}
.jobAdListings2.profiledCompanies li:hover > a,
.jobAdListings2.categoryJobadsCombo li:hover > a,
.jobAdListings2.mgmt li:hover > a,
.jobAdListings2.profiled li:hover > a,
.jobAdListings2.recruiting li:hover > a,
