    display: block;
    height: 20px;
        line-height: 20px;
    width: 100%;
    white-space: nowrap;
    overflow: hidden;
}
.jobAdListings2.categoryJobadsCombo li span.f-small,
.jobAdListings2.mgmt li span.f-small,
.jobAdListings2.profiled li span.f-small {
