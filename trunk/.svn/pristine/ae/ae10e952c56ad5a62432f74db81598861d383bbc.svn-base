    font-size: 12px;
}

.jobAdListings2.profiledCompanies .list,
.jobAdListings2.categoryJobadsCombo .list,
.jobAdListings2.mgmt .list,
.jobAdListings2.profiled .list,
.jobAdListings2.recruiting .list {
    width: 180px;
    height: 378px!important;
}

.jobAdListings2.profiledCompanies li,
.jobAdListings2.categoryJobadsCombo li,
.jobAdListings2.mgmt li,
.jobAdListings2.profiled li,
.jobAdListings2.recruiting li {
    float: left;
    width: 180px;
    margin-bottom: 5px;
}

.span4 .jobAdListings2 li,
.span6 .jobAdListings2 li,
.span10 .jobAdListings2 li {
