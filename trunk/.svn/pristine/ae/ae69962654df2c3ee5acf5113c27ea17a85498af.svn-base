<div id="companyText" class="bodyText">
    {{if (jobad.company.showCompanyName === true)}}
        <span id="companyName">${jobad.company.name}</span>
    {{/if}}
    {{if (jobad.company.companyLogos[0] !== undefined)}}
        {{if (jobad.company.url !== undefined)}}
            <a href="${jobad.company.url}">
        {{/if}}
            <img class="imageLeft" id="companyLogo"
                src="http://${lisaLevel}.aftenposten.no/utils/img.php?src=${jobad.company.companyLogos[0].companyLogoPath}&amp;maxWidth=${imgWidth}"
                alt="${jobad.company.name}"/>
        {{if (jobad.company.url !== undefined)}}
            </a>
        {{/if}}
    {{/if}}
    {{if (jobad.companyProfile !== undefined)}}
        {{if (jobad.companyProfile.images[0] !== undefined)}}
            <img class="imageRight" id="companyImage"
                 src="http://${lisaLevel}.aftenposten.no/utils/img.php?src=${jobad.companyProfile.images[0].imagePath}&amp;maxWidth=${imgWidth}"
                 alt=""/>
        {{/if}}
        {{if (jobad.companyProfile.companyProfileText !== undefined)}}
            {{html jobad.companyProfile.companyProfileText.xmlCDataContent}}
        {{/if}}
    {{/if}}
    {{if (jobad.status !== 'PUBLISHED')}}
        <p>Annonsen er ikke aktiv</p>
    {{/if}}
</div>

<div id="adText" class="bodyText">
    <h1>${jobad.adTitle}</h1>
    {{if (jobad.adLeadText !== undefined)}}
        <p class="leadText">{{html jobad.adLeadText.xmlCDataContent}}</p>
    {{/if}}

    <a class="applyPosition" href="${jobad.applicationUrlEscenic}" title="${applicationTypeText}">${applicationTypeText}</a>

    {{html jobad.adText.xmlCDataContent}}

    {{if (jobad.generic1 !== undefined)}}
        {{html jobad.generic1.xmlCDataContent}}
    {{/if}}

    <table>
        <tr>
            <th>Firma</th>
            <td>${jobad.company.name}<br/>
            <address>{{if (jobad.address !== undefined)}}
                {{if (jobad.address.addressLine1 !== undefined)}}
                    ${jobad.address.addressLine1}<br/>
                {{/if}}
                ${jobad.address.postNumber.postNumberName} ${jobad.address.postNumber.city}
                {{if jobad.address.country.name !== 'Norge'}}
                    <br/>${jobad.address.country.name}
                {{/if}}
            {{/if}}
                </address>
            </td>
        </tr>
        {{if (jobad.firstEmployee !== undefined)}}
        <tr>
            <th>Kontakt</th>
            <td>${jobad.firstEmployee.completeName}
            {{if (jobad.firstEmployee.phone !== undefined)}}
                <br/>${jobad.firstEmployee.phone}
            {{/if}}
            {{if (jobad.firstEmployee.mobilePhone !== undefined)}}
                <br/>${jobad.firstEmployee.mobilePhone}
            {{/if}}
            {{if (jobad.firstEmployee.email !== undefined)}}
                <br/><a href="mailto:${jobad.firstEmployee.email}">Send epost</a>
            {{/if}}
            </td>
        </tr>
        {{/if}}
        {{if (jobad.secondEmployee !== undefined)}}
        <tr>
            <th>Kontakt</th>
            <td>${jobad.secondEmployee.completeName}
            {{if (jobad.secondEmployee.phone !== undefined)}}
                <br/>${jobad.secondEmployee.phone}
            {{/if}}
            {{if (jobad.secondEmployee.mobilePhone !== undefined)}}
                <br/>${jobad.secondEmployee.mobilePhone}
            {{/if}}
            {{if (jobad.secondEmployee.email !== undefined)}}
                <br/><a href="mailto:${jobad.secondEmployee.email}">Send epost</a>
            {{/if}}
            </td>
        </tr>
        {{/if}}
        {{if (jobad.accessionText !== undefined)}}
        <tr>
            <th>Tiltredelse</th>
            <td>${jobad.accessionText}</td>
        </tr>
        {{/if}}
        {{if (jobad.hoursText !== undefined)}}
        <tr>
            <th>Omfang</th>
            <td>${jobad.hoursText}</td>
        </tr>
        {{/if}}
        {{if (jobad.contractText !== undefined)}}
        <tr>
            <th>Varighet</th>
            <td>${jobad.contractText}</td>
        </tr>
        {{/if}}
        {{if (jobad.sectorText !== undefined)}}
        <tr>
            <th>Sektor</th>
            <td>${jobad.sectorText}</td>
        </tr>
        {{/if}}
        {{if (jobad.applicationClosingText !== undefined)}}
        <tr>
            <th>S&oslash;knadsfrist</th>
            <td>${jobad.applicationClosingText}</td>
        </tr>
        {{/if}}
    </table>

    <a class="applyPosition" href="${jobad.applicationUrlEscenic}" title="${applicationTypeText}">${applicationTypeText}</a>

    <a class="otherPositions" href="${adUrl}?companyId=${jobad.company.companyId}" title="Vis oversikt over stillinger fra ${jobad.company.name}">Andre stillinger fra <strong>${jobad.company.name}</strong></a>

    <img class="statImage" width="0" height="0" src="${rubrikkUrl}stat/clk.htm?id=${jobad.jobAdId}&parentid=${jobad.company.companyId}&type=JOB_AD${publicationNameUC}&rnd=${time}" alt=""/>
</div>