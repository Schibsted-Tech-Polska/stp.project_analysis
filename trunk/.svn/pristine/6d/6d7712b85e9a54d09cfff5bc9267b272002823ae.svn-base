    font-weight: bold;
    margin-top:6px;
    text-transform: uppercase;
}

/* right ad banner */
#adRight {
    margin-left: 0;
}

/* Tip us */
.tipUs {
    display: inline-block;
    background: #FD0501;
    color: #fff;
    border: 0 none;
    box-shadow: none;
    font-size: 14px;
    line-height: 1.6em;
    margin: 10px 0;
    padding: 0 12px;
    border-radius: 9px;
}
.tipUs .prefix {
    background: transparent;
    filter: none;
    background-image: none;
    border-radius: 0;
    position: static;
    width: auto;
    text-transform: uppercase;
    margin-right: 10px;
}
.tipUs .prefix-inner {
    font-size: 14px;
    margin: 0;
    position: static;
    text-align: left;
    text-shadow:none;
}
.tipUs a {
    color: #fff;
    font-weight: normal;
}

#page .tipUs .prefix-inner a:hover,
.tipUs a:hover {
    text-decoration: underline;
}
.tipUs .prefix a,
.tipUs .label {
    font-weight: bold;
    text-transform: uppercase;
}
.tipUs .tipType {
    margin-right: 10px;
}
p.moreToFollow {
    color: #F50400;
}

/* red widgets headers */
.widget header {
