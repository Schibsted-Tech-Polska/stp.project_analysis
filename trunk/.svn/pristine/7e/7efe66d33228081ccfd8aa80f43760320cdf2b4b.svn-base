/* Start: Politifact css*/
.truthinessSmall {
    background:url(/skins/global/gfx/politiFact/politiFact-icons.png) no-repeat -120px 0;
    float: left;
    margin-right: 10px;
    overflow: hidden;
    text-indent: 59px;
    width: 59px;
    height: 61px;
}
.truthinessSmall.false {
    background-position:-120px -71px;
}
.truthinessSmall.someFalse{
    background-position:-120px -142px;
}
.truthinessSmall.neutral {
    background-position:-120px -213px;
}
.truthinessSmall.mostlyTrue {
    background-position:-120px -284px;
}
.truthinessSmall.true {
    background-position:-120px -355px;
}

.sectionPage .media .politiFact, .list .media {
    border:0;
    display:block;
    overflow:hidden;
    margin-bottom:4px;
}

.truthiness,
.truthiness a{
    background:url(/skins/global/gfx/politiFact/politiFact-icons.png) no-repeat 0 0;
    overflow:hidden;
    text-indent: -10000px;
    width:110px;
    height:112px;
}
.truthiness.false,
.truthiness.false a{
    background-position:0 -134px;
}
.truthiness.someFalse,
.truthiness.someFalse a{
    background-position:0 -268px;
}
.truthiness.neutral,
.truthiness.neutral a{
    background-position:0 -402px;
}
.truthiness.mostlyTrue,
.truthiness.mostlyTrue a{
    background-position:0 -536px;
}
.truthiness.true,
.truthiness.true a{
    background-position:0 -664px;
}

.politiFactList{
    clear: both;
}

.politiFact .span4 h2{
    font-size: 17px;
}
#page .truthiness.left{
    margin-right: 10px;
}

/* End: Politifact css*/