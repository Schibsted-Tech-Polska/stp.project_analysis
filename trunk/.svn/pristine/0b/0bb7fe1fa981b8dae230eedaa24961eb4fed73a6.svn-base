h1.appfarmHeading {
    background: none repeat scroll 0 0 white;
    color: #02328E;
    display: block;
    font-family: Prelude,Verdana,Arial,"Droid Sans",sans-serif;
    font-size: 12px;
    font-weight: normal;
    margin: 0 auto -6px;
    position: relative;
    text-align: center;
    text-decoration: none;
    text-transform: uppercase;
    width: 100px;
    z-index: 100;
}
.jcarousel-skin-tango a {
    font-weight: normal;
    font-size: 10px;
    font-family: Prelude, Verdana, Arial, "Droid Sans", sans-serif;
    text-decoration: none;
    text-transform: uppercase;
    color: #02328e;
    -webkit-text-size-adjust: none;
}
.jcarousel-skin-tango img {
    border: 0;
}
.jcarousel-skin-tango .jcarousel-container {
    -moz-border-radius: 3px;
    background: #fff;
    border: 1px solid #aebbcc;
}
.jcarousel-skin-tango .jcarousel-direction-rtl {
    direction: rtl;
}
.jcarousel-skin-tango .jcarousel-container-horizontal {
    padding: 20px 25px;
    padding-left: 30px;
}
.jcarousel-skin-tango .jcarousel-item {
    text-align: center;
    -webkit-user-select:none; /* disable user select (safari) when clicking the navigation arrows quickly */
    -moz-user-select:none; /* disable user select (firefox) when clicking the navigation arrows quickly */
}
.jcarousel-skin-tango .jcarousel-item-horizontal {
    margin-left: 0;
    padding-right: 10px; /* need to use padding in order for it to scroll properly on iPhone */
}
.jcarousel-skin-tango .jcarousel-item-placeholder {
    background: #fff;
    color: #000;
}
/*
 Horizontal Buttons
*/
.jcarousel-skin-tango .jcarousel-next-horizontal {
    position: absolute;
    top: 43px;
    right: 5px;
    width: 20px;
    height: 20px;
    cursor: pointer;
    background: transparent url('/skins/global/gfx/icons/next-horizontal.png') no-repeat 0 0;
}
.jcarousel-skin-tango .jcarousel-direction-rtl .jcarousel-next-horizontal {
    left: 5px;
    right: auto;
    background-image: url('/skins/global/gfx/icons/prev-horizontal.png');
}
.jcarousel-skin-tango .jcarousel-next-horizontal:hover {
    background-position: -20px 0;
}
.jcarousel-skin-tango .jcarousel-next-horizontal:active {
    background-position: -40px 0;
}
.jcarousel-skin-tango .jcarousel-next-disabled-horizontal,
.jcarousel-skin-tango .jcarousel-next-disabled-horizontal:hover,
.jcarousel-skin-tango .jcarousel-next-disabled-horizontal:active {
    cursor: default;
    background-position: -60px 0;
}
.jcarousel-skin-tango .jcarousel-prev-horizontal {
    position: absolute;
    top: 43px;
    left: 5px;
    width: 20px;
    height: 20px;
    cursor: pointer;
    background: transparent url('/skins/global/gfx/icons/prev-horizontal.png') no-repeat 0 0;
}
.jcarousel-skin-tango .jcarousel-direction-rtl .jcarousel-prev-horizontal {
    left: auto;
    right: 5px;
    background-image: url('/skins/global/gfx/icons/next-horizontal.png');
}
.jcarousel-skin-tango .jcarousel-prev-horizontal:hover {
    background-position: -20px 0;
}
.jcarousel-skin-tango .jcarousel-prev-horizontal:active {
    background-position: -40px 0;
}
.jcarousel-skin-tango .jcarousel-prev-disabled-horizontal,
.jcarousel-skin-tango .jcarousel-prev-disabled-horizontal:hover,
.jcarousel-skin-tango .jcarousel-prev-disabled-horizontal:active {
    cursor: default;
    background-position: -60px 0;
}