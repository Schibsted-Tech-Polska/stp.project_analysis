.slideshow {
    overflow-x:auto;
        overflow-y:hidden;
    position: relative;
}

.mnoSlideshow.slideshow {
    overflow:hidden;
    overflow-x:hidden;
        overflow-y:hidden;
}

.mobileSlideshow {
	padding: 0;
}

.slideshow-wrapper {
    margin-bottom:20px;
}

.slideshow-element {
    display: block;
}

.slideshow-wrapper {
    background:#333;
    border:1px solid #666;
    overflow:hidden;
}

.slideshow-wrapper .slideshow-list {
    overflow:hidden;
    padding:0;
    position: relative;width: 100000px;
        min-height:432px;
}

.mobileSlideshow .slideshow-wrapper {
    margin-bottom: 5px;
}

.slideshow-element {
    float:left;
}

.slideShowItemContainer {
    position:relative;
    overflow:hidden;
}
.slideShowItemContainer.heightImg{
    text-align:center;
}
.slideShowItemContainer img.originalImage{
    max-height: 500px;
}

.slideshow-element .slideShowItemContainer img {
    vertical-align:middle;
}

.w580 .slideshow-list .noCaption {
    max-width:580px;
}

.w580 .slideshow-list .widthCaption {
     max-width:580px;
}

.w980 .slideshow-list .widthCaption {
    max-width:780px;
}
.w980 .slideshow-list .heightImg.widthCaption {
    width: auto;
    border-left: 1px solid #666;
}

.slideShowItemContainer .caption {
    text-align:left;
}

.w980 .slideShowItemContainer .caption {
    background:#f0f0f0;
    border-left:1px solid #666;
    position:absolute;
        top:0;
        bottom:0;
        left:780px;
    margin:0;
    width:199px;
}

.slideShowItemContainer .caption-inner {
    line-height:1.22;
    position:absolute;
        bottom:10px;
        left:10px;
    width:180px;
}
.w580 .slideShowItemContainer .caption {
    bottom: 10px;
    left: 490px;
    margin: 0;
    position: absolute;
    width: 75px;
}

.w980 .slideshow-list .map{
    width:978px;
        height:551px;
}

.w980.hasCaption .slideshow-list .map{
    width:978px;
        height:440px;
}

.slideShowItemContainer a.link{
    background-position: 230px 109px;
    background-repeat: no-repeat;
    bottom: 0;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
    display:block;
    cursor:pointer;
}
.slideShowItemContainer a.playButton{
    opacity: 0.5;    
}

.slideShowPrev,
.slideShowNext {
    background:#333;
    background:rgba(0,0,0,.8);
    border:2px solid #fff;
         -moz-border-radius:5px;
        -webkit-border-radius:5px;
        border-radius:5px;
    -moz-box-shadow:0 0 5px #ccc;
        -webkit-box-shadow:0 0 5px #ccc;
        box-shadow:0 0 5px #ccc;
    color:#fff;
    cursor:pointer;
    font-size:30px;
    line-height:40px;
        text-shadow: 0 1px 3px #666;
    position:absolute;
        top:40px;
        left:-10000px;
    width:40px;
        height:40px;
}
.slideShowPrev:hover,
.slideShowNext:hover {
    background:#000;
    background:rgba(0,0,0,.5);
}
.load .slideShowPrev.enabled,
.slideshowContainer:hover .slideShowPrev.enabled {
    left:5px;
}

.load .slideShowNext.enabled,
.slideshowContainer:hover .slideShowNext.enabled {
    left:auto;
    right:5px;
    text-align: right;
}

.ie .slideShowNext,
.ie .slideShowPrev {
    behavior: url(/skins/global/PIE.htc);
}

.slideshow .counter {
    letter-spacing:1px;
    text-align:center;
    position:absolute;
        bottom:0;
    width:100%;
}

.slideshow .counter a {
    color:#666;
}

.slideshow .counter a:hover {
    color:#000;
}

.mnoSlideshow {
    overflow:hidden;
    width:100%;
}


/* slideshowlist */
.slideshowsList .description{
    padding-bottom:40px !important;
}

.slideshowsList > h3{
    font-size: 25px;
    margin-bottom: 20px;
}

.slideshowsList .gridUnit a{
    display:block;
    margin-bottom:10px;
}

.slideshowsList .gridUnit a{
    display:block;
    margin-bottom:10px;
}

.slideshowsList .gridUnit a.r{
    display: inline-block;
    height: 15px;
    line-height: 15px;
    margin-bottom: 5px;
    text-align: left;
    font-size: 10px;
    font-weight:bold;
}

.widget.mnoSlideshow .thumbnails a {
    display: block;
    float: left;
    margin-right: 3px;

    user-select: none;
    -moz-user-select: none;
    -khtml-user-select: none;
    -webkit-user-select: none;
    -o-user-select: none;
}
.widget.mnoSlideshow .counterWrapper {
    margin: 0 1px 10px;
}
.widget.mnoSlideshow .thumbsNext,
.widget.mnoSlideshow .thumbsPrev {
    width: 30px;
    background: #f8f8f8;
    font-size: 20px;
    line-height: 20px;
    vertical-align: middle;
    position: relative;

    cursor: pointer;
    user-select: none;
    -moz-user-select: none;
    -khtml-user-select: none;
    -webkit-user-select: none;
    -o-user-select: none;
}
.widget.mnoSlideshow .thumbsNext {
    float: right;
    content: '&#x25b6;'
}
.widget.mnoSlideshow .thumbsPrev {
    float: left;
    content: '&#x25c0;'
}
.widget.mnoSlideshow .thumbsNext div,
.widget.mnoSlideshow .thumbsPrev div {
    position: absolute;
    top: 50%;
    line-height: 22px;
    font-size: 22px;
    margin: -12px 5px 0;
}