/* override tabletQuickFix.css from globals */
@media only screen and (max-device-width: 1024px) and (min-device-width: 600px) and (orientation: portrait) {
    body div#page {
        font-size: 1em;
    }

}

@media only screen and (max-device-width: 1024px) and (min-device-width: 600px) {
    #page #header {
        margin-top: 0;
    }
}

@media only screen and (max-width: 599px) and (min-width: 481px){
	
     .tipOssMobile, .eAvisMobile{
     	margin-left:1.2em;
     }
    .logoMobile, .sideMenuMobile,.weatherMobile,  .tipOssMobile, .eAvisMobile{
       width: auto;
        overflow:visible;
	   
    }
	.weatherMobile{
		float:right;
		margin-right: 0.1em;
	}
}

@media only screen and (max-width: 480px){
	
	.tipOssMobile, .eAvisMobile{
        display:none ;
    }
	.logoMobile, .sideMenuMobile,  .weatherMobile{
       width: auto;
    }
	.weatherMobile{
        float:right;
        margin-right: 0.1em;
    }
}


/* header  */
#header {
    margin-bottom:0.625em;
}
.logoMenu {
    background:#000000;
}

.logoMenu a{
    color:#646464;
}
.logoMenu .subMenu{

    background:#ededed;
    font-size:0.79em;
    padding-top: 0.2em;
    padding-left:0.5em;
}
.sideMenuTrigger {
    width: 1.8em;
    height: 3em;
    display: block;
    position: relative;
    padding-left: 2em;
    cursor: pointer;
    float: left;
}
.sideMenuTrigger:before {
    content: '';
    position: absolute;
    top: 0.8em;
    left: 0.85em;
    width: 2em;
    height: 0.375em;
    border-top: 0.813em double #fff;
    border-bottom: 0.250em solid #fff;
}

.logo a {
    background:url("/skins/publications/farsund/gfx/lister-mobile.png") no-repeat 0 0;
    width: 7.313em;
    height: 2.438em;
    background-size: 7.313em 2.438em;
    display: block;
    margin-left: 0.2em;
    margin-top: 0.6em;
	color: white;
}
.logo a span {
    visibility: hidden;
	color: white;
}
#sideMenu {
    background-color: #000;
}
#sideMenu nav.sideMenuGeneral  {
    font-size: 1em;
}
#sideMenu nav li {
    line-height: 2em;
}
#sideMenu nav li .link {
    padding: 0 0.625em;
}
#sideMenu nav a:active {
    color: #F50400;
}

/* layout*/
button,
.button {
    background-color:#014687;
}

.gridUnit img {
    background:none;    
}


/* typography */
body, h1, h2 {
    font-family: "Helvetica Neue", Calibri, Helvetica, Arial, sans-serif;
}
/* Tip us */
.tipUs {
    display: inline-block;
    background: #FD0501;
    color: #fff;
    font-size: .75rem;
    line-height: 1.6em;
    margin: .625rem 0;
    padding: 0 .5em;
    border-radius: .563rem;
}
.tipUs a {
    color: #fff;
    font-weight:bold;
    text-decoration: underline;
}
.tipUs .prefix {
    text-transform: uppercase;
    margin-right: .5em;
}
.tipUs .prefix a,
.tipUs .label {
    font-weight: bold;
    text-transform: uppercase;
    color: #fff;
}
.tipUs .tipType {
    margin-right: .5em;
}
p.moreToFollow {
    color: #F50400;
}

.logoMenu .mobileMenu{
    float:left;
}

.mobileWeather{
    float:right;
}

.mobileWeather .stormWeather-today{
    display: block;
    padding-top: 0;
}

.mobileWeather .stormWeather-symbol{
    display: block;
    margin-top: 9px;
    margin-right:4px;
    float:right;
}

.mobileWeather .stormWeather-symbol img{
    display:block;
    float:right;
    margin-left:5px;
        margin-top:-5px;
}

.mainMenu li {
    border-left: 1px solid #00467D;
    border-right: 1px solid #005DA6;
    height: 2.4em;
    padding-top: 0.5em;
}

.mainMenu a {
    color:#fff;
    font-size:1.2em;
}

/* ads */
.adMarker {
    color:#666;
    display:block;
    font-size:10px;
    margin-left:5px;
}

a {
    color: #014687;
}

section[aria-hidden=true],
div[aria-hidden=true],
article[aria-hidden=true]{
    display:none;   
}


.factboxes img {
    float: right;
    max-width:50%;
        height:auto;
    margin-bottom: 0;
    margin-left: 8px;

}

#colophon {
    background:#f0f0f0;
    border-top:1px solid #999;
    overflow:hidden;
    width:100%;
}

.header h2 {
    font-size:12px;
        font-weight:bold;
    margin-bottom:0;
}


.comments a {
    text-decoration:none;
    background: url(/skins/global/gfx/icons/sprite_15.png) 0 -76px;
    background-repeat: no-repeat;
    padding-left: 20px;
    font-size: 10px;
}

.negativ .comments a {
    color:#fff;
    text-transform:uppercase;
}


.pager {
    list-style:none;
    overflow:hidden;
    padding:5px;
    width:100%;
}
.pager li {
    display:-moz-inline-stack;
    display:inline-block;
    margin-bottom: 20px;
    margin-right:5px;
}
.ie .pager li {
    display:inline;
    position:relative;
}

.pager .button.active {
    background-color:#ccc;
    border:1px solid #666;
    color:#333;
}

/* Les ogsaa */
.relatedArticles {
    border-bottom: 3px solid #005395;
    margin-bottom: 10px;
}

.relatedArticles .header {
    margin: 0;
}

.relatedArticles .header h2 {
    font-size: 0.85em;
}

.relatedArticles>div {
    padding-top: 5px;
    padding-bottom: 3px;
    background-color: #e0e0e0;
}

.relatedArticles .inline-image img {
    float: left;
    margin: 0 7px 2px 5px;
}

.relatedArticles .inline-image a {
    width: 40%;
}

.relatedArticles h3 {
    line-height: 1.15em;
    margin-bottom: 4px;
    font-size: 0.95em;
}

.relatedArticles p {
    margin: 0 5px;
    line-height: 1.15em;
    font-size: 0.95em;

}

#header .searchField input,
#footer .searchField input {
    height: 17px;
    width: 230px;
}

#header .mobileHeader .searchField form,
#footer .mobileHeader .searchField form {
    float:left;
    margin: 8px auto;
    overflow: hidden;
    position: static;
    top: auto;
}

/* Mobileswipe START. Move to own CSS */
.mobileSwipe {
    padding:0;
}

.mobileSwipe .dxwNoSwipeNavPrevious img, .mobileSwipe .dxwNoSwipeNavNext img {
    width:20px;
}

.mobileSwipe .dxwNoSwipeNavPrevious img {
    float: left;
}

.mobileSwipe .dxwNoSwipeNavNext img {
    float: right;
}

.mobileSwipe .indexContainer {
    margin: auto;
    text-align: center;
}

.mobileSwipe .caption .caption-inner h2 {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 10px;
    font-weight: normal;
    line-height: normal;
    margin: 0 5px;
}

/* When only one picture in swipe Mobile swipe widget*/
.mobileStoryContent .caption h2 {
    font-family: Arial,Helvetica,sans-serif;
    font-size: 10px;
    font-weight: normal;
}


.tip, .eavis a {
	color: white;
	display: block;
}

.tip span {
	font-size: 0.6em;
	background: url("/skins/publications/farsund/gfx/white-phone.png");
	display: block;
	background-repeat: no-repeat;
	height: 3.5em;
	width: 100%;
	background-position: 0px -1px;
	text-transform: uppercase;
	line-height: 3.5em;
	padding-left: 3em;
	margin-top: 1em;
}

.eavis a{
	font-size: 1.3em;
	position: relative;
	top: 0.5em;
}

.mobile.nowForecastForHeader{
    display: block ;
    height: 3em ;
}
 .mobile.nowForecastForHeader  a {
    color: #fff ;
    position:relative;
    top:0.8em ;
}

.mobile.nowForecastForHeader  .weather-icon{
    vertical-align: middle; 
}
			
.mobile.nowForecastForHeader  a span{
    line-height:24px;
}

 .mobile.nowForecastForHeader .temp{
 	color:white !important;
	font-weight: normal !important;
 }

#page .gridUnit .rowTheme.hasThemeHeadline {
    padding-top: 30px;
}

/* mobile dialog box */
#mnoDialogBg {
    background: black;
    background: rgba(0, 0, 0, .8);
    filter: alpha(opacity=80);
    position: absolute;
    top: 0;
    left: 0;
    z-Index: 1000;
    width: 100%;
    height: 100%;
}
#mnoDialog {
    background: white;
    border: 5px solid #666;
    border: 5px solid rgba(0, 0, 0, .5);
    -moz-border-radius: 12px;
    -webkit-border-radius: 12px;
    border-radius: 12px;
    -moz-box-shadow: 0 0 20px #000;
    -webkit-box-shadow: 0 0 20px black;
    box-shadow: 0 0 20px black;
    position: fixed;
    z-index: 1001;
    padding: 10px;
    overflow-x: visible;
}
#mnoDialogClose{
    background:#333;
    border:2px solid #fff;
    -moz-border-radius:12px;
    -webkit-border-radius:12px;
    border-radius:12px;
    -moz-box-shadow:0 1px 2px #000;
    -webkit-box-shadow:0 1px 2px #000;
    box-shadow:0 1px 2px #000;
    color:#fff;
    cursor:pointer;
    font-size:14px;
    font-weight:bold;
    line-height:20px;
    text-shadow:0 1px 2px #000;
    text-align:center;
    position:absolute;
    right:-15px;
    top:-15px;
    width:20px;
    height:20px
}

/* Mobileswipe END. Move to own CSS */