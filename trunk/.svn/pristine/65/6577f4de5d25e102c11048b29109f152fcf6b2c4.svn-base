#header {
    margin-left:0;
        margin-top: 20px;
    padding: 0;
    width:100%;
}

#header a {
    text-decoration:none;
}

#header a:hover {
    text-decoration: underline;
}

@media only screen and (min-device-width : 600px) and (max-device-width : 1024px) {
    #header{
        margin-top:30px;
    }
}