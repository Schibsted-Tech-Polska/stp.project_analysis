{{each items}}
<article class="informationItem">
    <header>
        <h2>
            {{html displayTime.time}}
            {{if (displayTime.date)}}
                <span>{{html displayTime.date}}</span>
            {{/if}}
        </h2>
    </header>
    <div class="itemContent">
        {{html displayContent}}
    </div>
</article>
{{/each}}
