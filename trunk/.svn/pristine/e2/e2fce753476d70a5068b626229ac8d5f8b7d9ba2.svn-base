/* yr disclaimer styling */
.weather .yrDisclaimer,
.weather .yrDisclaimer a{
    color: #6D869F;
    font-family: "Lucida Grande","Lucida Sans Unicode",Verdana,Arial,sans-serif;
    font-size: 12px;
    text-decoration:none;
}

/* temperature styling */
.weather .temp {
    font-weight: bold;
}

.weather .temp.cold {
    color: #007aa6;
}
.weather .temp.warm {
    color: #990000;
}

/* Single Line list */
.weather.singleLineList li {
    padding-top:8px;
    padding-bottom:6px;
}

.weather.singleLineList .content li{
    padding: 5px 15px 3px 3px;
}

.weather.singleLineList li a,
.weather.singleLineList li span{
    float:left;
}

.span10 .weather.mobile.singleLineList .name,
.span3 .weather.singleLineList .name{
    width:160px;
}

.span4 .weather.singleLineList .name{
    width: 260px;
}

.span4 .tabs.weather.singleLineList .name{
    width: 230px;
}

.weather.singleLineList .name{
    overflow: hidden;
    -o-text-overflow:ellipsis;
        -ms-text-overflow: ellipsis;
        text-overflow:ellipsis;
    width: 360px;
    white-space: nowrap;
}

.weather.singleLineList .localtime{
    width: 50px;
    font-size: 0.8em;
    color: #333;
}

.weather.singleLineList .weather-icon {
    margin-top:-5px;
}

.yrCopy {
    overflow: hidden;
    margin-bottom:15px;
    width:100%;
}

.yrCopy .yrBox,
.yrCopy .metNrkBox {
    float:left;
}

.yrCopy .yrBox {
    border-right: 1px solid #E4E3DC;
    margin-right:10px;
    padding-right:10px;
    width:125px;
}

.yrCopy .metNrkBox {
    width:180px;
}

.yrCopy span {
    color:#666;
    display:block;
    font-size:12px;
    text-transform:uppercase;
    width:100%;
}

.yrCopy a {
    background: url(/skins/global/gfx/weather/yrNrkMet.png) no-repeat;
    float:left;
    height:30px;
    text-indent:-10000px;
}

.yrCopy .yr {
    width:121px;
}

.yrCopy .nrk {
    background-position: 0 -30px;
    width:85px;
}

.yrCopy .met {
    background-position: 0 -60px;
    margin-right:10px;
    width:84px;
}


