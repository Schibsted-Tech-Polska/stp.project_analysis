.rotator .content {
    background: #D0D6E0;
}

.rotator .item a {
    color:#000;
}

.rotator .item.active a {
    font-weight: bold;
}

.rotator .item.active .large a {
    font-weight: normal;
}

.rotator h2 {
    font-family: arial, sans-serif;
}

.rotator .large h2 {
    font-family: Georgia, serif;
}
