<ul class="articleList">
    {{each(i,value) items}}
        <li>
            <a href="?adNo={{html value.adNo}}&adType={{html value.adTypeCode}}&view=json">
                {{if (value.adTypeCode === 'OBI')}}
                    {{html value.lastName}}, {{html value.firstName}}
                {{else}}
                    {{html value.lastName}}
                {{/if}}
            </a>
            <span class="right">{{html value.startDate}}</span>
        </li>
    {{/each}}
</ul>