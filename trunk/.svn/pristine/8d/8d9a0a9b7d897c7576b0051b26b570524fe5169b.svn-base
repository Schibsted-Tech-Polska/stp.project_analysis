.tvGuide header{
    background : url(/skins/publications/farsund/gfx/logos_sprite.png);
    background-repeat:no-repeat;
    padding-left: 83px;
    border:none;
    margin-bottom: 10px;
}

.tvGuide header h2 {
    padding-left:10px;
    background-color: black;
    font-weight:bold;
    color:white;
    text-transform:uppercase;
    line-height: 22px;
}
.tvGuide header a, .tvGuide header a:hover {
    color: #fff;
}
.tvGuide header a.floatRight {
    margin-right: 5px;
}