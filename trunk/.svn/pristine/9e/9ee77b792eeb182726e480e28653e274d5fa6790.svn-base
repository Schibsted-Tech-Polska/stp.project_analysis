.tabList .tabs ul {
    display:block;
    background:none ;
    border: 0 ;

}
.tabList .tabs ul{
    margin-bottom:-1px;
    position:relative;
    z-index:2;
}

.tabList .tabs li{
    padding:3px 5px;
    font-size:12px;
        text-align:center;
    display:block;
    float:left;
    height:14px;
    opacity:1;
    position:relative;
        z-index:2;
    margin:0 5px -1px 0;
}

.tabList .tabs .ui-state-default{
    border:1px solid #7FA1C4;
    background-color:#7FA1C4;
}

.tabList .tabs .ui-state-active{
    border:1px solid #7FA1C4;
        border-bottom:1px solid #fff;
    background-color:#fff;
}

.tabList .tabs .ui-state-default a {
    color:#fff;
}
.tabList .tabs .ui-state-active a {
    color:#7FA1C4;
}

.tabList .content{
    border:1px solid #7FA1C4;
    height:367px;
    overflow:hidden;
    padding:5px;
    position:relative;
        z-index:1;
}

.tabList .content:hover {
    overflow-y:auto;
}

.tabList .content li {
    padding-right:19px;
}

.tabList .content:hover li {
    padding-right:0;
}

.tabList .content p {
    margin-bottom:0;
}

.tabList .ui-tabs-hide{
    display:none;
}
.tabList h2 a{
    font-family:arial, sans-serif;
        color: #005395;
        font-size:16px;
        text-decoration: underline;
}
.tabList h2 {
    margin-bottom: 0;
}

.accordionList .reviewContainer,
.tabList .reviewContainer
{
    border-bottom: 1px dotted #ccc;
    padding-bottom: 5px;
}
.accordionList .last,
.tabList .last{
    border-bottom: 0;
}

.tabList .image{
    margin-left:0 !important;
}

.tabList .tabSectionLink{
    text-transform: uppercase;
    text-align: center;
    display: block;
    margin-top: 10px;
}