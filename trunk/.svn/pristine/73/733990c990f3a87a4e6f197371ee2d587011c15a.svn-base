template.wireframe.markup.file=default
skin=escenic-times

# The following two parameters are required for article comments widget
comments.unmoderated.forumId=
comments.moderated.forumId=

eae.logger.url=http://localhost:8080/analysis-logger/Logger
eae.queryservice.url=http://localhost:8080/analysis-qs/QueryService
