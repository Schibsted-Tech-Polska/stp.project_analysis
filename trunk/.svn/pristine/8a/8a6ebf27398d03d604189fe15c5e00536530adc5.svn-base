.tvGuide .tvGuideSearch {
    margin-top: 20px;
}

/* tv guide schedule starts here */
.tvGuide dl {
    margin: 0 0 0 20px;
}

.tvGuide dt{
    border-bottom:3px solid #3C3C3C;
    margin-bottom: 10px;
    padding-bottom: 5px;
}
.tvGuide dd{
    margin: 0 0 2px;
}

.tvGuide .hiddenDescription{
    display:none;
    font-size:12px;
}

.tvGuide .hiddenDescription img{
    float:left;
    width:40px;
}

.tvGuide .selected{
    background-color:#FFCC00;
    margin-right: -20px;
}

.tvGuide .last .selected{
    margin-right:0;    
}

.tvGuide .passed,
.tvGuide .passed a,
.tvGuide .passed .time{
    color: #ccc;
}

.tvGuide dd .time{
    color:#666;
    display: block;
    float:left;
}

.tvGuide a{
    color:#000;
    text-decoration:none;
}
.tvGuide a:hover{
    color: inherit;
    text-decoration:underline;
}

.tvGuide dd .title{
    display: block;
    float:left;
    padding-left: 4px;
    width: 140px;
}
/* tv guide schedule ends here */


.tvGuide .hidden{
    display:none;
}


/* icons */
.tvGuide .tvChannelIcon {
    background: url(/skins/global/gfx/tvguide/icons_35x50_NY.png) no-repeat 0 0;
    display: block;
    height: 35px;
        width: 50px;
}
.tvGuide .iconNRK1 {
    background-position: 0 -35px;
}
.tvGuide .iconNRK2 {
    background-position: 0 -70px;
}
.tvGuide .iconTV2NORGE {
    background-position: 0 -103px;
}
.tvGuide .iconTV3NORGE {
    background-position: 0 -140px;
}
.tvGuide .iconTVNORGE {
    background-position: 0 -175px;
}
.tvGuide .iconTVNORGEMAX {
    background-position: 0 -210px;
}