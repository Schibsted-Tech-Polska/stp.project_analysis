*/

.artTheme,
.rowTheme {
    background-color: #e8ebf0;
        border-bottom: 4px solid #123466;
        border-top: 4px solid #123466;
    position:relative;
    text-shadow:none;
}

.gridRow.rowTheme {
    margin-bottom:15px;
}

#viewport .rowTheme.noThemeHeadline,
#viewport .artTheme.noThemeHeadline {
    border-top: 0 none;
    border-bottom: 0 none;
}

.rowTheme .themeHeadline h1,
.artTheme .themeHeadline h2 {
    background-color:#123466;
    color: #fff;
    font-family:"Arial Black", arial, sans-serif;
        font-size: 12px;
        font-weight: bold;
        text-shadow: none;
        text-transform: uppercase;
        line-height: 20px;
    margin: 0;
    padding:  0 10px;
    position:absolute;
        left:0;
        top:-4px;
}

.artTheme.hasThemeHeadline,
.rowTheme.hasThemeHeadline {
    padding-top:23px;
}

.artTheme.noThemeHeadline,
.artTheme.noThemeHeadline h2,
.artTheme.noThemeHeadline h3,
.artTheme.noThemeHeadline .header h3,
.artTheme.noThemeHeadline a,
.artTheme.noThemeHeadline a:hover,
.artTheme.noThemeHeadline a:visited,
.artTheme.noThemeHeadline a:active,
.rowTheme.noThemeHeadline,
.rowTheme.noThemeHeadline h2,
.rowTheme.noThemeHeadline h3,
.rowTheme.noThemeHeadline .header h3,
.rowTheme.noThemeHeadline a,
.rowTheme.noThemeHeadline a:hover,
.rowTheme.noThemeHeadline a:visited,
.rowTheme.noThemeHeadline a:active{
    color:#fff;
}

.artTheme.hasThemeHeadline,
.artTheme.hasThemeHeadline h2,
.artTheme.hasThemeHeadline h3,
.artTheme.hasThemeHeadline .header h3,
.artTheme.hasThemeHeadline a,
.artTheme.hasThemeHeadline a:hover,
.artTheme.hasThemeHeadline a:visited,
.artTheme.hasThemeHeadline a:active{
    color:#000;
}

.artTheme.hasThemeHeadline .themeHeadline h2 {
    color: #fff;
}

/* Negativ */
.negativ .themeHeadline h1,
.artTheme.negativ .themeHeadline h2 {
    background-color:#000;
}

.rowTheme.negativ.hasThemeHeadline,
.artTheme.negativ.hasThemeHeadline {
    background-color: #444;
    border-bottom: 4px solid #000;
    border-top: 4px solid #000;
    padding-top: 23px;
}

.artTheme.negativ {
    padding: 5px 10px;
}

.artTheme.negativ.hasThemeHeadline,
.artTheme.negativ.hasThemeHeadline h2,
.artTheme.negativ.hasThemeHeadline h3,
.artTheme.negativ.hasThemeHeadline .header h3,
.artTheme.negativ.hasThemeHeadline a,
.artTheme.negativ.hasThemeHeadline a:hover,
.artTheme.negativ.hasThemeHeadline a:visited,
.artTheme.negativ.hasThemeHeadline a:active{
    color:#fff;
}

/* blue - standard colors */
