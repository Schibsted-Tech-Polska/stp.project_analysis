.offCanvas.offCanvasLabel .highlight {
    background-image: -moz-linear-gradient(top, #990, #660); /* FF3.6 */
    background-image: -ms-linear-gradient(top,  #990, #660);
    background-image: -webkit-linear-gradient(top,  #990, #660);
    background-image: linear-gradient(to bottom,  #990, #660);
}
