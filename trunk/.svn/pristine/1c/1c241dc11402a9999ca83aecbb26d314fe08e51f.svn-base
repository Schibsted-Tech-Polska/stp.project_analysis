/* themes */
.artTheme,
.rowTheme {
    text-shadow:none;
}

.artTheme,
.rowTheme {
    padding-top:10px;
        padding-bottom:10px;
}

.artTheme.hasThemeHeadline {
    padding-top: 0;
}

.themeHeadline {
    display:none;
}

.artTheme .themeHeadline,
.rowTheme .themeHeadline {
    color:#fff;
    display: block;
    font-size: 16px;
        text-shadow: 0 -1px rgba(51,51,51,0.5);
        text-transform: uppercase;
        line-height: 25px;
    height: 25px;
    padding-left:10px;
}

.rowTheme .themeHeadline {
    margin: 0 -16px 10px 0;
}
.artTheme .themeHeadline {
    margin: 0 -10px 10px;
}

.relatedContents.stories header.header h2,
.singleLineList header.header h2,
.singleLineList header.header h2 a,
.hasThemeHeadline .widget header.header h2,
.hasThemeHeadline .widget header.header h2 a {
    background: transparent;
    color:#009fda;
    font-family: georgia, serif;
        font-size: 14px;
        font-weight: normal;
        font-style: italic;
    text-transform: none;
        text-shadow: none;
    padding: 0;
        margin: 0;
    height: auto;
    -moz-box-shadow: none;
        -webkit-box-shadow: none;
        box-shadow: none;
}

.hasThemeHeadline .gridUnit {
    margin-bottom: 15px;
}


.artTheme .media.block {
    margin: -10px -10px 10px -10px;
}
.artTheme.themeBrokenTitle .media.block {
    margin: 0;
}
.artTheme .media.block.imageLeft {
    margin-right: 10px;
}
.artTheme .media.block.imageRight {
    margin-left: 10px;
}

.sectionNavigator,
.storyContent .navigation.pagetitle h5,
.mobileStoryContent .navigation.pagetitle h5,
.themeHeadline,
.menuBar {
    background-color:#009fda;
    background-image:-moz-linear-gradient(bottom, #00659c, #009fda);
        background-image:-webkit-gradient(linear, left bottom, left top, from(#00659c), to(#009fda));
        background-image:-webkit-linear-gradient(bottom, #00659c, #009fda);
        background-image:-o-linear-gradient(bottom, #00659c, #009fda);
        background-image:linear-gradient(bottom, #00659c, #009fda);
}
.ie9 .sectionNavigator,
.ie9 .storyContent .navigation.pagetitle h5,
.iemobile .mobileStoryContent .navigation.pagetitle h5,
.ie9 .themeHeadline,
.ie9 .menuBar,
.iemobile .menuBar {
    background-image:url(/skins/global/gfx/gradient.jsp?type=linear&y1=0&x1=0&y2=100&x2=0&noCircle=true&colorStop=009fda|0|1,00659c|100|1);
}
.storyContent .navigation.pagetitle:after,
.rowTheme .themeHeadline:after {
    border-top: 6px solid #333;
        border-right: 6px solid transparent;
    content: "";
    display: block;
    position: absolute;
        right: -6px;
}
.rowTheme.themeBrokenTitle .themeHeadline:after,
.artTheme .themeHeadline:after {
    display: none;
}

.artTheme.hasThemeHeadline {
    background-color: #ededed;
}
.artTheme.hasThemeHeadline.themeBrokenTitle {
    background: transparent;
}

/* magazine */
.magazine .span10 h1,
.magazine .span10 h2 {
    font-size: 50px;
}

.magazine article,
.magazine article.artTheme {
	background-color: #ededed;
    display: table;
	padding: 10px 10px 10px 10px;
        margin: 10px 0;
    width: 100%;
}
.magazine.magazineImageLeft article,
.magazine.magazineImageLeft article.artTheme {
    padding-left: 0;
}
.magazine.magazineImageRight article,
.magazine.magazineImageRight article.artTheme {
    padding-right: 0;
}
.magazine .media.block.imageLeft {
	margin: -20px 10px -20px 0;
}
.magazine .media.block.imageRight {
	margin: -20px 0 -20px 10px;
}

.magazine .hasThemeHeadline .themeHeadline {
    background-color: transparent;
        background-image: none;
    box-shadow: none;
    display: block;
    padding: 0 10px 10px;
        margin-bottom: 0;
}
.ie9 .magazine .hasThemeHeadline .themeHeadline {
    background-image: none;
}
.magazine.magazineImageLeft .hasThemeHeadline .themeHeadline {
    float: right;
}
.magazine.magazineImageRight .hasThemeHeadline .themeHeadline {
    float: left;
}

.magazine .hasThemeHeadline .themeHeadline h2 {
    color: #000;
    text-shadow: none;
}
.magazine .hasThemeHeadline.artTheme .themeHeadline h2 {
    color: #fff;
    text-shadow: 0 -1px rgba(51, 51, 51, 0.5);
}

.magazine .hasThemeHeadline.debatt .themeHeadline h2:before {
    background-image: url(/skins/publications/fvn/gfx/sprite.png);
        background-repeat: no-repeat;
        background-position: -90px -6px;
    content: "";
    display: block;
    float: left;
    width: 20px;
        height: 20px;
}

.magazine.magazineImageLeft h2[itemprop=teaserTitle] {
    clear: right;
}
.magazine.magazineImageRight h2[itemprop=teaserTitle] {
    clear: left;
}

.magazine .artTheme,
.magazine .artTheme a,
.magazine .artTheme h2 a {
    color: #fff;
    text-shadow: 0 -1px rgba(0,0,0,0.3);
}
.magazine.magazineImageRight .artTheme,
.magazine.magazineImageRight .artTheme a,
.magazine.magazineImageRight .artTheme h2 a {
    text-align: right;
}

/* zebraStriped */
.zebraStripedDark {
    background-color: #f4f4f4;
}
.zebraStripedDark .gridRow:nth-child(odd),
.zebraStripedDark li:nth-child(odd) {
    border-bottom: 1px solid #fff;
}
.zebraStripedDark .gridRow:nth-child(even),
.zebraStripedDark li:nth-child(even) {
    background-color: #e9e9e9;
    border-bottom: 1px solid #d4d4d4;
}

.zebra > *:nth-child(odd),
.zebraStripedLight .article:nth-child(odd),
.zebraStripedLight .gridRow:nth-child(odd),
.zebraStripedLight li:nth-child(odd) {
    background-color: #f4f4f4;
    border-bottom: 1px solid #d4d4d4 !important;
}
.zebra > *:nth-child(even),
.zebraStripedLight .article:nth-child(even),
.zebraStripedLight .gridRow:nth-child(even),
.zebraStripedLight li:nth-child(even) {
    background-color: transparent;
    border-bottom: 1px solid #fff !important;
}

.zebraStripedLight .articleList li,
.zebraStripedDark .articleList li {
    padding: 8px 10px 8px 5px;
        margin-bottom: 0;
    width: inherit;
}
.zebraStripedLight .articleList li p,
.zebraStripedDark .articleList li p{
    font-family: georgia, times, serif;
        font-size: 15px;
        line-height: 1.4em;
    margin-bottom: 0;
}
.zebraStripedLight .articleList li h2,
.zebraStripedDark .articleList li h2{
    font-weight: normal;
}

