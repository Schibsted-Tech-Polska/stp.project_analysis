.rowTheme.dinmat,
.ie .rowTheme.dinmat,
.ie9 .rowTheme.dinmat,
.rowTheme.blogg,
.ie .rowTheme.blogg,
.ie9 .rowTheme.blogg {
    background-color: transparent;
    filter: progid:DXImageTransform.Microsoft.gradient(enabled=false);
        -ms-filter: "progid:DXImageTransform.Microsoft.gradient(enabled=false)";
        background-image: none;
    border-top: 0 none;
    border-bottom: 0 none;
}

.rowTheme.sprek .themeHeadline h1,
.rowTheme.dinmat .themeHeadline h1,
.rowTheme.blogg .themeHeadline h1 {
