<ul>
{{each(i,specialCategory) items}}
    <li>
        <a id="partnerLogo"
           onclick="xt_med('C','${xitiId}','annSelsk::logo_jobb_${specialCategory.name}','S')"
           href="${rubrikkUrl}statistics/?companyId=${specialCategory.companies[0].companyId}&amp;objectType=COMPANY_LOGO_PARTNERLIST${statAppend}&amp;redirect={{html Base64.encode(specialCategory.url)}}">
            {{if specialCategory.companies[0].companyLogos[0]}}
                <img alt="Logo ${specialCategory.name}" src="http://${lisaLevel}.aftenposten.no/utils/img.php?src=${specialCategory.companies[0].companyLogos[0].companyLogoPath}&amp;maxWidth=175&amp;maxHeight=175"/>
            {{else}}
                ${specialCategory.name}
            {{/if}}
        </a>
        {{if onlyLinks != 'true'}}
            <p class="info">
            {{if specialCategory.specialCategoryLogos[0]}}
                <img src="http://${lisaLevel}.aftenposten.no/utils/img.php?src=${specialCategory.specialCategoryLogos[0].specialCategoryLogoPath}&amp;maxWidth=80&amp;maxHeight=80" alt="Logo ${specialCategory.name}"/>
            {{/if}}
            {{if specialCategory.text !== undefined && specialCategory.text.xmlCDataContent}}
                {{html specialCategory.text.xmlCDataContent}}
            {{/if}}
            </p>
        {{/if}}
        <ul>
            {{if specialCategory.linkText1}}
            <li>
                <a id="parterLink1"
                   onclick="xt_med('C','${xitiId}','annSelsk::lenke1_jobb_${specialCategory.name}','S')"
                   href="${rubrikkUrl}statistics/?companyId=${specialCategory.companies[0].companyId}&amp;objectType=COMPANY_LINK_PARTNERLIST${statAppend}&amp;redirect={{html Base64.encode(specialCategory.linkUrl1)}}">
                    {{html specialCategory.linkText1}}
                </a>
            </li>
            {{/if}}
            {{if specialCategory.linkText2}}
            <li>
                <a id="partnerLink2"
                   onclick="xt_med('C','${xitiId}','annSelsk::lenke2_jobb_${specialCategory.name}','S')"
                   href="${rubrikkUrl}statistics/?companyId=${specialCategory.companies[0].companyId}&amp;objectType=COMPANY_LINK_PARTNERLIST${statAppend}&amp;redirect={{html Base64.encode(specialCategory.linkUrl2)}}">
                    {{html specialCategory.linkText2}}
                </a>
            </li>
            {{/if}}
            {{if specialCategory.linkText3}}
            <li>
                <a id="partnerLink3"
                   onclick="xt_med('C','${xitiId}','annSelsk::lenke3_jobb_${specialCategory.name}','S')"
                   href="${rubrikkUrl}statistics/?companyId=${specialCategory.companies[0].companyId}&amp;objectType=COMPANY_LINK_PARTNERLIST${statAppend}&amp;redirect={{html Base64.encode(specialCategory.linkUrl3)}}">
                    {{html specialCategory.linkText3}}
                </a>
            </li>
            {{/if}}
            {{if specialCategory.linkText4}}
            <li>
                <a id="partnerLink4"
                   onclick="xt_med('C','${xitiId}','annSelsk::lenke4_jobb_${specialCategory.name}','S')"
                   href="${rubrikkUrl}statistics/?companyId=${specialCategory.companies[0].companyId}&amp;objectType=COMPANY_LINK_PARTNERLIST${statAppend}&amp;redirect={{html Base64.encode(specialCategory.linkUrl4)}}">
                    {{html specialCategory.linkText4}}
                </a>
            </li>
            {{/if}}
        </ul>
        {{if specialCategory.linkUrl}}
        <p class="more">
            <a id="partnerLink"
               onclick="xt_med('C','${xitiId}','annSelsk::sitelink_jobb_${specialCategory.name}','S')"
               href="${rubrikkUrl}statistics/?companyId=${specialCategory.companies[0].companyId}&amp;objectType=COMPANY_LINK_PARTNERLIST${statAppend}&amp;redirect={{html Base64.encode(specialCategory.linkUrl)}}">
                {{html specialCategory.linkText}}</a>
        </p>
        {{/if}}
    </li>
{{/each}}
</ul>