{{each items}}
    <li>
        <img class="imageLeft" src="${profile_image_url}" alt="${from_user_name}" />
        <h2>
            <a class="twitterUser" href="http://www.twitter.com/${from_user}">
                ${from_user_name} <span class="small">@${from_user}</span>
            </a>
        </h2>
        <div class="createdAt"></div>
        <p>{{html text}}</p>
    </li>
{{/each}}