<table class="mnoDatePicker">
    <thead>
        <tr class="monthYear">
            <th colspan="4">
                <select name="month">
                    {{each months}}
                        <option {{html selected}} value="${value}">{{html name}}</option>
                    {{/each}}
                </select>
            </th>
            <th colspan="3">
                <select name="year">
                    {{each years}}
                        <option {{html selected}} value="${value}">{{html name}}</option>
                    {{/each}}
                </select>
            </th>
        </tr>
        <tr class="days">
            {{each dayLabels}}
                <th>${$value}</th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        {{each row}}
        <tr>
            {{each cell}}
                <td {{html current}}>
                    {{html time}}
                </td>
            {{/each}}
        </tr>
        {{/each}}
    </tbody>
</table>
