<h3>Ledige stillinger:</h3>
<div class="list">
  <ul>
    {{each(i, value) items}}
        <li>
            <a onclick="xt_med('C', '${xitiId}','jobbdirekte_dagens_utvalgte::module_clicked','S')" href=" ${value.url}">
                {{if value.logo}}
                    <div><img src="{{html value.logo}}" alt="{{html value.company}}" height="23px" /></div>
                {{/if}}
                <p class="ellipsis">{{html value.title}}</p>
            </a>
        </li>
    {{/each}}
  </ul>
</div>